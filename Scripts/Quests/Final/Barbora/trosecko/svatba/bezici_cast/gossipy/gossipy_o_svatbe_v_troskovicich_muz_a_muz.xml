								<Response Role="SVATBA_GOSSIP_MUZ_1_TROSKOVICE" ReferenceLength="4.1">
									<Text StringName="goss_svatba_go_no_jo_prej_YYkX" Text="No jo. Prej se vožral jak hovado, a do někoho se tam pustil." />
