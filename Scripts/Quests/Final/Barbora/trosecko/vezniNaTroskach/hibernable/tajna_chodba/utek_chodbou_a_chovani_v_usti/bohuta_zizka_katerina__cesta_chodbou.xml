							<DesignName StringName="bohuta_zi_seq1_ax7x" Text="seq1" />
							<Elements>
								<Response Role="BOHUTA_NPC" ReferenceLength="4.51">
									<Text StringName="bohu_bohuta_npc_dlouha_ja_SzRe" Text="Vždycky mě zajímalo, jak taková tajná chodba pod hradem vypadá." />
									<Commands>
										<MoodCommand Delay="0" Mood="noScope" />
										<MoodCommand Role="KATERINA" Delay="0" Mood="noScope" />
										<MoodCommand Role="JAN_ZIZKA_Z_TROCNOVA" Delay="0" Mood="noScope" />
										<FacialMoodCommand Role="KATERINA" FacialMood="happy1" />
										<AnimationCommand Delay="2.88" FragmentId="ADLG_Gesture" Guid="05cb510e-ec58-4891-91be-f1006f98dbd9" Variant="0" />
										<AnimationCommand Role="KATERINA" Delay="4.3" FragmentId="ADLG_Confusion" Guid="4cc561b1-68ad-4b66-9a64-6ebf3c70dcd8" Variant="0" />
										<LookAtCommand Delay="0.02" CustomTarget="player" />
									</Commands>
								</Response>
								<Response Role="KATERINA">
									<Text StringName="bohu_zizka_drzte_hubu__oba_JtLS" Text="Copak? Snad nejseš zklamanej?" />
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="5.3">
									<Text StringName="bohu_bohuta_npc_jak_se_to_e06y" Text="Jak se to vezme. Nad žejdlíkem piva sem si v hospodě vyslech leccos." />
									<Commands>
										<AnimationCommand Delay="3.33" FragmentId="ADLG_Gesture" Guid="22f8b7f8-02df-4e21-a4d5-f2c5264ceb7c" Variant="1" />
									</Commands>
								</Response>
								<Response Role="KATERINA" ReferenceLength="3.6">
									<Text StringName="bohu_katerina_a_co_by_si__mO8Z" Text="A co si čekal? Podzemní palác?" />
									<Commands>
										<AnimationCommand Delay="1.66" FragmentId="ADLG_Gesture" Guid="78537867-75d3-4d70-9c93-79400706ad91" Variant="3" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="4.2">
									<Text StringName="bohu_bohuta_npc_to_ne_ale_GJZB" Text="To ne ale nějaký... No bludiště a trochu toho svítícího mechu. " />
									<Commands>
										<FacialMoodCommand FacialMood="happy1" />
										<AnimationCommand Delay="0.12" FragmentId="ADLG_Disagree_light" Guid="64f57625-bf5d-e5fd-c4d0-407e66f32194" Variant="0" />
									</Commands>
									<ReviewNotesConcept>
										<Item Date="2022-05-19 14:08:24" UserName="richard.malatinsky" Text="Obecná věc, ale zejména tady, když je to ingame, u kterýho ty NPC jdou a mluvěj: Maximálně zkracovat délku replik. Tady bych to rozdělil klidně do dvou, po jedný větě. Bude to vypadat líp. " AcceptState="Accepted" />
									</ReviewNotesConcept>
								</Response>
								<Response Role="BOHUTA_NPC" StartDelay="0.15" ReferenceLength="8.21">
									<Text StringName="bohu_bohuta_npc_clovek_si_xs83" Text="Člověk si tajnou chodbu představuje spíš jako minotaurův labyrint na Krétě než jako... no... tohle." />
									<Commands>
										<AnimationCommand Delay="7.39" FragmentId="ADLG_Gesture" Guid="325ed9cf-90d6-454b-8e27-f8eac8e3df73" Variant="2" />
										<AnimationCommand Delay="3.56" FragmentId="ADLG_Gesture" Guid="5a97cf50-508f-4386-9bee-b2aa7a27d34f" Variant="5" />
									</Commands>
								</Response>
								<Response Role="KATERINA">
									<Text StringName="bohu_katerina_minoco_xaYP" Text="Mino-co?" />
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="6.83">
									<Text StringName="bohu_bohuta_npc_minotauru_PL1D" Text="Minotaurus je příšera, chlap co má hlavu bejka a bydlí v labyrintu - neboli bludišti..." />
									<Commands>
										<AnimationCommand Delay="1.99" FragmentId="ADLG_Gesture" Guid="6df35932-8ed5-4ad3-bf69-05478433ffe5" Variant="7" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" StartDelay="0.19" ReferenceLength="4.06">
									<Text StringName="bohu_bohuta_npc_je_to_sta_Eiix" Text="Je to stará antická pověst. Čet jsem o ní v semináři..." />
									<Commands>
										<AnimationCommand Delay="2.54" FragmentId="ADLG_Gesture" Guid="81594070-b736-4892-9b2f-a60a24d28f8b" Variant="9" />
									</Commands>
								</Response>
								<Response Role="KATERINA" ReferenceLength="6.37">
									<Parenthetical>
										<Text StringName="bohu_katerina_pobavene_MfJs" Text="pobaveně" />
									</Parenthetical>
									<Text StringName="bohu_katerina_chlapu_co_m_C861" Text="Chlapů co mají bejčí hlavu znám spoustu! A nemusim ani číst starý pověsti v semináři... " />
									<Commands>
										<AnimationCommand Delay="0.74" FragmentId="ADLG_Gesture" Guid="ce65b0af-9a8a-4874-a723-4ade834c1214" Variant="7" />
									</Commands>
								</Response>
								<Response Role="KATERINA" ReferenceLength="2.14">
									<Parenthetical>
										<Text StringName="bohu_katerina_popichne_zi_TXEi" Text="popíchne Žižku" />
									</Parenthetical>
									<Text StringName="bohu_katerina_vid_zizko_SuoH" Text="Viď, Žižko." />
									<Commands>
										<AnimationCommand Delay="1.11" FragmentId="ADLG_Gesture" Guid="9ed59c26-3e05-9e49-b81e-8ef0b56bbdf9" Variant="6" />
									</Commands>
								</Response>
								<Response Role="JAN_ZIZKA_Z_TROCNOVA">
									<Parenthetical>
										<Text StringName="bohu_zizka_ztezka_4Plh" Text="ztěžka" />
									</Parenthetical>
									<Text StringName="bohu_zizka_katerino___waHo" Text="Kateřino... " />
								</Response>
								<Response Role="KATERINA" ReferenceLength="9.11">
									<Text StringName="bohu_katerina_a_jak_k_tom_LUFB" Text="A jak k tomu vůbec tenhle &quot;minorautrus&quot; přišel, že měl bejčí hlavu? Třeba bych pak pochopila jak se to těm mužskejm stane. " />
									<Commands>
										<AnimationCommand Delay="6.5" FragmentId="ADLG_Frustration" Guid="13cc3bf0-ca49-52e1-f3c2-92a3b8574ea1" Variant="0" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="7.68">
									<Parenthetical>
										<Text StringName="bohu_bohuta_npc_opatrne_50pF" Text="opatrně" />
									</Parenthetical>
									<Text StringName="bohu_bohuta_npc_no__jeho__ndNs" Text="No... Jeho matka, královna Pásifaé, vobcovala s bájnym bílym bejkem." />
									<Commands>
										<AnimationCommand Delay="4.97" FragmentId="ADLG_Gesture" Guid="b66abf65-b022-4dc5-b5c5-5cefbe1fb098" Variant="13" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="4.3">
									<Text StringName="bohu_bohuta_npc_ale_tim_s_HxzH" Text="Ale tim samozřejmě nechci říct, že by to tak bylo tady u hejtmana..." />
									<Commands>
										<AnimationCommand Delay="2.52" FragmentId="ADLG_Gesture" Guid="c15cd05d-9a14-4e82-bbc0-f37125934ca8" Variant="14" />
										<AnimationCommand Role="JAN_ZIZKA_Z_TROCNOVA" Delay="4.17" FragmentId="ADLG_Agree" Guid="d3cd78e7-e21f-616e-5809-9af2836ad5d4" Variant="2" />
										<LookAtCommand Delay="1.37" LookAtRole="JAN_ZIZKA_Z_TROCNOVA" TransitionDuration="1" />
									</Commands>
								</Response>
								<Response Role="JAN_ZIZKA_Z_TROCNOVA">
									<Text StringName="bohu_zizka_to_dobre_delas_75yl" Text="To dobře děláš... " />
								</Response>
								<Response Role="KATERINA" ReferenceLength="4.85">
									<Text StringName="bohu_katerina_a_jak_dopad_79hp" Text="A jak dopad tenhle... mino... chlapobejk?" />
									<Commands>
										<AnimationCommand Delay="4.18" FragmentId="ADLG_Nod" Guid="45484834-e025-a236-9a0b-9cfa3bf201cd" Variant="0" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="6.9">
									<Text StringName="bohu_bohuta_npc_zabil_ho__GTQ0" Text="Zabil ho athénskej princ Théseus. Ale musim dodat, že mu docela pomohla princezna Ariadna." />
									<Commands>
										<AnimationCommand Delay="3.14" FragmentId="ADLG_Surprised_light" Guid="716cab22-8d72-dd05-6011-17d78a96f760" Variant="1" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" StartDelay="0.13" ReferenceLength="5">
									<Text StringName="bohu_bohuta_npc_darovala__JtEv" Text="Darovala mu kouzelnou nit, aby našel cestu zpátky z labyrintu." />
									<Commands>
										<AnimationCommand Delay="3.64" FragmentId="ADLG_Gesture" Guid="a4d90be2-e0ed-4b9e-b2c5-c424c4995599" Variant="11" />
									</Commands>
								</Response>
								<Response Role="KATERINA" ReferenceLength="6.89">
									<Text StringName="bohu_katerina_prisery_a_p_uzRX" Text="Příšery a princezny s kouzelnou nití... Na to by vás v semináři užilo, co?" />
									<Commands>
										<AnimationCommand Delay="4.65" FragmentId="ADLG_Gesture" Guid="ce65b0af-9a8a-4874-a723-4ade834c1214" Variant="7" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="3.36">
									<Parenthetical>
										<Text StringName="bohu_bohuta_npc_pobavene__gn0w" Text="pobaveně uraženě" />
									</Parenthetical>
									<Text StringName="bohu_bohuta_npc_ja_si_na__4nVs" Text="Já si na tom piloval svoji řečtinu!" />
									<Commands>
										<AnimationCommand Delay="2.01" FragmentId="ADLG_Emphasis" Guid="75340ec6-f2fb-ce70-0dfb-07c2a0a46bc2" Variant="0" />
									</Commands>
								</Response>
								<Response Role="KATERINA" ReferenceLength="7.45">
									<Text StringName="bohu_katerina_me_je_jasny_lbIT" Text="Mně je úplně jasný, co sis piloval u příběhu, ve kterym bájná královna vobcuje s bejkem!" />
									<Commands>
										<AnimationCommand Delay="1.62" FragmentId="ADLG_Gesture" Guid="93cb0a18-ec6a-4abd-93c8-f73f948c18b2" Variant="5" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="10.41">
									<Text StringName="bohu_bohuta_npc_ha__no_ty_Ezei" Text="Ha... ty se teda nezdáš. Ale můžu tě ujistit, že v semináři je na tohle úplně jiná literatura." />
									<Commands>
										<AnimationCommand Delay="1.35" FragmentId="ADLG_Discard" Guid="8795ff76-a4e6-6b43-1b68-21d53f7bf938" Variant="0" />
									</Commands>
								</Response>
								<Response Role="KATERINA" ReferenceLength="2.37">
									<Parenthetical>
										<Text StringName="bohu_katerina_se_zajmem_4D2s" Text="se zájmem" />
									</Parenthetical>
									<Text StringName="bohu_katerina_vazne_jaka_fMcA" Text="Vážně? Jaká?" />
									<Commands>
										<AnimationCommand Delay="1.51" FragmentId="ADLG_Gesture" Guid="37505f52-0a9c-4e80-ba9d-d4d3901a7294" Variant="1" />
									</Commands>
								</Response>
								<Response Role="JAN_ZIZKA_Z_TROCNOVA" ReferenceLength="4.84">
									<Parenthetical>
										<Text StringName="bohu_zizka_vycerpane_0zW7" Text="vyčerpaně" />
									</Parenthetical>
									<Text StringName="bohu_zizka_kristepane_drz_Yvjr" Text="Kristepane, držte už hubu! Ještě nás někdo uslyší." />
									<Commands>
										<AnimationCommand Delay="1.17" FragmentId="ADLG_Frustration" Guid="741afe3e-65ff-47af-7c27-b0e8c9f2c97e" Variant="0" />
										<AnimationCommand Role="KATERINA" Delay="4.71" FragmentId="ADLG_Agree" Guid="a9e6c5ff-95e4-433d-139e-d626a0e38289" Variant="2" />
									</Commands>
								</Response>
								<Response Role="KATERINA" ReferenceLength="4.09">
									<Parenthetical>
										<Text StringName="bohu_katerina_popichuje_b_E9BH" Text="popichuje Bohutu" />
									</Parenthetical>
									<Text StringName="bohu_katerina_jan_ma_prav_JV4j" Text="Jan má pravdu. Už těch prasáren radši nech, otče..." />
									<Commands>
										<AnimationCommand Role="BOHUTA_NPC" Delay="4.06" FragmentId="ADLG_Me" Guid="a7764e21-a2a6-72f5-00c7-55ee60c3b772" Variant="2" />
									</Commands>
								</Response>
								<Response Role="BOHUTA_NPC" ReferenceLength="3.47">
									<Parenthetical>
										<Text StringName="bohu_bohuta_npc_ohradi_se_9qDz" Text="ohradí se" />
									</Parenthetical>
									<Text StringName="bohu_bohuta_npc_ale__ja_j_lyjD" Text="Prasáren? Ale já jsem... Ty si chtěla vědět..." />
									<Commands>
										<AnimationCommand Role="JAN_ZIZKA_Z_TROCNOVA" Delay="3.47" FragmentId="ADLG_Gesture" Guid="6d127636-0c20-4ed0-a489-ef307e7b7591" Variant="6" />
									</Commands>
								</Response>
								<Response Role="JAN_ZIZKA_Z_TROCNOVA">
									<Parenthetical>
										<Text StringName="bohu_zizka_vcerpane_8v6A" Text="vyčerpaně" />
									</Parenthetical>
									<Text StringName="bohu_zizka_ticho_8xSR" Text="Ticho!" />
								</Response>
							</Elements>
						</Sequence>
