			<QuestName StringName="qname_rasuvucen_2t2m" Text="Zakázané ovoce" />
			<QuestDescription StringName="qdesc_rasuvucen_1n5P" Text="U Krejzla se člověk nenudí. Jen co jsem dodělal jednu práci, hned mi přistála nová. Tentokrát mám zjistit, kde bych na Trosecku našel sanytrovou jámu. Mlynář má totiž zálusk na kopu ledku, kterej chce použít na oživení golema z mouky. " />
			<Objectives>
				<Objective TypeT="hledaniJamy" Name="najdi_sanytrovou_jamu_a_ochutnej_zda_je_v_ni_ledek_">
					<LocalizedName StringName="rasuvucen_najdi_sanytrovo_vEdw" Text="Najdi sanytrovou jámu a ochutnej, zda je v ní ledek. " />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Started" />
						<EnumLog Type="Updated" Name="UpdatedAfterFail">
							<Log StringName="najdi_san_na_jamu_jsem_do_zLqa" Text="S hledáním jámy mi měl pomoct místní hrobník, ten mi ale už rozhodně nepomůže. Budu ji teď muset najít sám! Kde by jen mohla být? Napadá mě... Troskovický písař je chytrý chlapík, co má přehled. Možná by mohl něco vědět." />
						</EnumLog>
						<EnumLog Type="Updated" Name="UpdatedAfterDiscovery" Marker="trigger_ledek">
							<Log StringName="najdi_san_konecne_stojim__6pLn" Text="Jámu jsem konečně našel. Jediné co zbývá je ochutnat ten zpropadený ledek. Jak se to říká? Zacpat si během toho nos?" />
						</EnumLog>
						<EnumLog Type="Completed" Name="Completed">
							<Log StringName="najdi_san_jamu_jsem_konec_P5px" Text="Jámu jsem nakonec našel a ochutnáním ověřil, že je v ní skutečně ledek. To poslední bych rád co nejdřív zapomněl." />
						</EnumLog>
						<EnumLog Type="None" Name="Aborted" />
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailableAbortable" Name="promluv_s_bonkou">
					<LocalizedName StringName="rasuvucen_promluv_s_bonko_E7KH" Text="Promluv s Bonkou." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Started" Marker="bonka">
							<Log StringName="promluv_s_s_ukolem_mi_ma__xYj9" Text="S úkolem mi má pomoct Krejzlova děvečka Bonka. Prý je v Brlohu, což je mlynářské skladiště poblíž Podsemínského mlýna. Stačí jít podél jižní strany rybníka. Po cestě bych prý mohl natrhat nějaké bylinky, což by mojí nové kumpánce udělalo zajisté radost." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Completed">
							<Log StringName="promluv_s_seznamil_jsem_s_2nmR" Text="Seznámil jsem se s Bonkou, za kterou mě Krejzl poslal do jejich tajného skladiště, aby mi pomohla s hledání jámy. S pacholkem Zinkem už ledek sbírali, ale stále ho mají nedostatek. " />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Failed">
							<Log StringName="promluv_s_bonka_je_mrtva__2HFz" Text="Bonka je mrtvá. Překážka pro splnění mého úkolu to ale není." />
						</EnumLog>
						<EnumLog Type="None" Name="Aborted" />
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="navstiv_hrobnika">
					<LocalizedName StringName="rasuvucen_navstiv_hrobnika_bNax" Text="Navštiv hrobníka." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="ras">
							<Log StringName="navstiv_h_bonka_mi_prozra_d4mJ" Text="Bonka mi prozradila, že tvůrcem sanytrové jámy není nikdo jiný než Troskovický hrobník. Navštívím ho u hřbitova za Troskovicemi. Je to prý kousek od řezníkovi chalupy, kde bych mohl koupit nějakou čerstvou obětinu v podobě salámu." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="navstiv_h_od_bonky_jsem_z_vvju" Text="Od Bonky jsem zjistil, že tvůrcem sanytrové jámy nebyl nikdo jiný než Troskovický hrobník Ignác. Ani ten mi ale nechtěl prozradit, kde je schovaná. " />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled" />
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="vyraz_s_hrobnikem_na_objizdku">
					<LocalizedName StringName="rasuvucen_vyraz_s_hrobnik_FKzv" Text="Vyraž s hrobníkem na objížďku." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="ras">
							<Log StringName="vyraz_s_h_budu_se_muset_s_qnLB" Text="Naštěstí mi slíbil, že mě tam doveze. K sanytrové jámě ale očividně vede dlouhá a trnitá cesta plná hrobničiny, se kterou budu chtě nechtě muset vypomoct. To jsem teda zvědavý co mě čeká. Ignác vyjíždí prý vždycky za rozbřesku. Do tý doby se můžu nějak připravit." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="vyraz_s_h_musel_jsem_se_s_RJBJ" Text="Naštěstí mi slíbil, že mě tam doveze. K sanytrové jámě ale vedla dlouhá a trnitá cesta plná hrobničiny, se kterou jsem musel chtě nechtě vypomoct. Vyrazili jsme za rozbřesku. Byl jsem hodně zvědavý co mě čeká." />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled" />
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="nasleduj_ignace_na_vuz">
					<LocalizedName StringName="rasuvucen_nasleduj_ignace_AkaV" Text="Následuj Ignáce na vůz." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="ras">
							<Log StringName="nasleduj__ignac_ma_byteln_Ha0Q" Text="Ignác má bytelný čtyřkolák tažený dvěma koňmi, takže se aspoň nebudeme muset vláčet pěšky. Škoda, že mě nenechá řídit." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done" />
						<EnumLog Type="Canceled" Name="Canceled" />
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="najdi_mrsinu_v_rozpadlem_statku">
					<LocalizedName StringName="rasuvucen_najdi_mrsinu_v__7KwE" Text="Najdi mršinu v rozpadlém statku." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="area_mrsina1">
							<Log StringName="najdi_mrs_hrobnik_se_s_ni_J74n" Text="Po krátké jízdě na voze jsme zastavili u rozpadlé chalupy ve Slatějově. Hrobník v ní má past na divokou zvěř a mým úkolem je obhlédnout, jestli se do ní něco chytlo.. Když tam byl Ignác posledně, překvapila ho tam smečka vlků. Určitě je na místě být ostražitý." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="najdi_mrs_hrobnik_se_s_ni_U6Mz" Text="Po krátké jízdě na voze jsme zastavili u rozpadlé chalupy ve Slatějově. Hrobník v ní měl past na divokou zvěř a mým úkolem bylo obhlédnout, jestli se do ní něco chytlo. Do chalupy jsem se dostal a našel tam jednu rozkládající se srnu." />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled" />
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="dones_mrsinu_na_vuz">
					<LocalizedName StringName="rasuvucen_dones_mrsinu_na_VRSV" Text="Dones mršinu na vůz." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="area_trialogAmbush">
							<Log StringName="dones_mrs_musim_tu_mrsinu_6VON" Text="Musím tu mršinu přinést na Ignácův vůz. Frkáček s Vosyčkou se rozhodli zastavit dál po cestě, takže se pěkně projdu." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="dones_mrs_pytel_jsem_pak__zrQu" Text="Mršinu jsem zabalil do pytle a odnesl na vůz, který tvrdohlaví koně zastavili dál po cestě." />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled" />
					</Logs>
				</Objective>
				<Objective TypeT="prepadeniNaCeste" Name="ochran_ignace_pred_pohunky">
					<LocalizedName StringName="rasuvucen_ochran_ignace_p_6J6F" Text="Ochraň Ignáce před pohůnky." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Started" Marker="ras">
							<Log StringName="ochran_ig_prepadeni_musim_9GGS" Text="Co to má znamenat? Nějací vidláci obstoupili vůz a nezní moc přátelsky. Měl bych dávat pozor, ať se Ignácovi nic nestane." />
						</EnumLog>
						<EnumLog Type="Completed" Name="DoneViolent">
							<Log StringName="ochran_ig_ze_nas_kvuli_bo_EELL" Text="Když jsem se vracel k vozu, všiml jsem si trojice chlapíků, kteří obestoupili vůz a působili nepřátelsky. Ukázalo se, že hledali Bonku se Zinkem, kteří tehdy při hledání sanytru nevybírali místním jenom chlívy . Neměl jsem náladu s nimi řešit, že s tím nemáme nic společného a pohůnci dostali pěkně za vyučenou." />
						</EnumLog>
						<EnumLog Type="Completed" Name="DoneNonViolent">
							<Log StringName="ochran_ig_ze_nas_kvuli_bo_qoMB" Text="Když jsem se vracel k vozu, všiml jsem si trojice chlapíků, kteří obestoupili vůz a působili nepřátelsky. Ukázalo se, že hledali Bonku se Zinkem, kteří při hledání sanytru nevybírali místním jenom chlívy. Nedorozumění se naštěstí vyřešilo bez zbytečnýho násilí. V tomhle případě jsme s Podsemínskými neměli nic společného." />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled" />
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="promluv_s_ignacem_po_statku_1">
					<LocalizedName StringName="rasuvucen_promluv_s_ignac_fuHj" Text="Promluv s Ignácem." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="ras">
							<Log StringName="promluv_s_neni_treba_se_t_uDwz" Text="Není třeba se tu zdržovat. Řeknu Ignácovi, že můžeme pokračovat." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done" />
						<EnumLog Type="Canceled" Name="Canceled" />
