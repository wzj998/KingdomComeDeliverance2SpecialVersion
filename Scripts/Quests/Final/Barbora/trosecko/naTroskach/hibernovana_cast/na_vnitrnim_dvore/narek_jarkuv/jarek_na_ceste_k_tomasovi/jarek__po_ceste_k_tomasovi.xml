	<Skald>
		<Dialog Name="jarek__po_ceste_k_tomasovi">
			<Text StringName="jarek__po_ceste_k_tomasovi_Z6IY" Text="Jarek - Po cestě k Tomášovi" />
			<Dialogue Type="ingame monolog" TechnicalStatus="Enabled" Hearing="35" Initiator="NonPlayer" GesturesSetDate="2023-11-03T13:32:09.7260667+01:00" GesturesSetResponseCount="12">
				<SelectedSouls>
					<SelectedSoul Role="STRAZNY_JAREK" Voice="christopherDukes" Type="Wave" Language="ENG" />
				</SelectedSouls>
				<Decision Name="dec1" Priority="MainQuestSubmission" Alias="naTroskach_jarekPoCesteKTomasovi">
					<Sequences>
						<Sequence EntryCondition="!ThisSequenceUsed()" EndType="EndDialogue" Name="seq1">
							<DesignName StringName="jarek__po_seq1_d40x" Text="seq1" />
							<Elements>
								<Response Role="STRAZNY_JAREK" ReferenceLength="3.67">
									<Text StringName="jare_strazny_j_felcar_se__Skwj" Text="Felčar se trochu zdráhal, když sem mu řek, že chci, aby Tomáše vytáhli nahoru." />
									<Commands>
										<AnimationCommand FragmentId="ADLG_Gesture" Guid="002ace53-8d31-4d61-b729-4bae5c80b2e5" Variant="0" DesiredDuration="1.94" />
										<AnimationCommand Delay="3.6" FragmentId="ADLG_Confusion" Guid="19e1b926-8753-4893-af45-10d39fa3a985" Variant="0" DesiredDuration="2.28" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK">
									<Text StringName="jare_strazny_j_raneny_ma__rLRG" Text="Raněnej má ležet co nejblíž kapli, to ví každej." />
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="5.63">
									<Text StringName="jare_strazny_j_pred_lety__qch8" Text="Před lety sem od jednoho mana slyšel, že se něco podobnýho stalo na Štramberku za markraběcích válek." />
									<Commands>
										<AnimationCommand Delay="1.84" FragmentId="ADLG_Gesture" Guid="8c8fca6c-9607-4c98-8f74-95922b507a56" Variant="16" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="9.02">
									<Text StringName="jare_strazny_j_purkrabi_h_bxjf" Text="Purkrabí hradu byl vážně raněnej. Nepřátelskej jezdec ho napích na kopí jak sele na rožeň a ještě ho táhnul půlkou lesa." />
									<Commands>
										<AnimationCommand Delay="4.83" FragmentId="ADLG_Gesture" Guid="649b3c66-46dc-4666-8a6c-7d8df90c80ea" Variant="10" DesiredDuration="1.92" />
										<AnimationCommand Delay="6.86" FragmentId="ADLG_Gesture" Guid="0229b273-f5e4-4cb2-a60f-332f72774af3" Variant="1" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="8.36">
									<Text StringName="jare_strazny_j_vsichni_si_v1Aq" Text="Všichni si mysleli, že se nedožije rána, ale hradní kaplan, nějakej Petronel, zavelel, že purkrabí musí bejt blízko Pánu, až nadejde jeho hodina." />
									<Commands>
										<AnimationCommand Delay="6.37" FragmentId="ADLG_Gesture" Guid="f897a1b3-c338-416e-88ea-7345a13432a2" Variant="26" />
										<AnimationCommand Delay="0.06" FragmentId="ADLG_Confusion" Guid="4fcec99e-67a8-4f82-9734-675a1804203e" Variant="3" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="5.99">
									<Text StringName="jare_strazny_j_na_strambe_4WT7" Text="Na Štramberku je taková hodně vysoká věž, místní ji říkaj Trouba nebo Trumpeta, protože člověku se skoro zdá, že se rozšiřuje nahoře." />
									<Commands>
										<AnimationCommand Delay="0.07" FragmentId="ADLG_Gesture" Guid="f15da38f-1fd7-45d6-9587-0b74338f13fc" Variant="24" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="4.6">
									<Text StringName="jare_strazny_j_na_vlasntn_uEdY" Text="Na vlastní voči sem ji viděl po cestě do Krakova, když sem doprovázel pana Bergova." />
									<Commands>
										<AnimationCommand Delay="0.11" FragmentId="ADLG_Nod" Guid="c05d1d1b-ecca-45b7-8434-c2d21c80187b" Variant="1" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="5.32">
									<Text StringName="jare_strazny_j_kdyz_tam_p_4ZjX" Text="Jak tam purkrabího i s kopím na Petronelův příkaz vynesli, tak se druhej den nestačili divit." />
									<Commands>
										<AnimationCommand Delay="1.21" FragmentId="ADLG_Gesture" Guid="f7b6478f-50ab-4615-975b-b70e831408c4" Variant="25" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="7.48">
									<Text StringName="jare_strazny_j_purkrabi_s_A2T6" Text="Purkrabí se zázračně uzdravil. V noci tam prej sestoupil anděl a vytáh purkrabímu kopí z rány." />
									<Commands>
										<AnimationCommand Delay="2.78" FragmentId="ADLG_Point" Guid="de38a264-7aaa-4ee9-b694-0e18a60419c4" Variant="5" DesiredDuration="1.6" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="3.91">
									<Text StringName="jare_strazny_j_od_ty_doby_47bc" Text="Od tý doby maj to kopí vystavený na hradě." />
									<Commands>
										<AnimationCommand Delay="0.07" FragmentId="ADLG_Gesture" Guid="34ac6648-6fd5-4005-84ec-632a56223892" Variant="5" DesiredDuration="1.64" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="5.28">
									<Text StringName="jare_strazny_j_ja_felcara_3T82" Text="Já felčara chápu, nechtěl, aby se Tomášovi přitížilo. Ono totiž vylízt na Pannu není žádná legrace." />
									<Commands>
										<AnimationCommand Delay="0.19" FragmentId="ADLG_Gesture" Guid="2f5b9a79-4e1b-4360-ac65-ce0ab1844c72" Variant="4" />
									</Commands>
								</Response>
								<Response Role="STRAZNY_JAREK" ReferenceLength="2.86">
									<Text StringName="jare_strazny_j_jenze_s_ch_Dijx" Text="Jenže s chlapama jsme se shodli, že to tak bude nejlepší." />
									<Commands>
										<AnimationCommand Delay="0.1" FragmentId="ADLG_Confusion" Guid="5da7fd8b-7cec-40c8-a723-f3cc47022f36" Variant="4" />
									</Commands>
								</Response>
							</Elements>
						</Sequence>
					</Sequences>
				</Decision>
