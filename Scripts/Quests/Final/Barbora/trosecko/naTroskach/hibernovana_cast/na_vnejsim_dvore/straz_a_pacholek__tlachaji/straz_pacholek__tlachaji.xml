								<Response Role="NA_TROSKACH_PACHOLEK" StartDelay="0.27" ReferenceLength="3.1">
									<Text StringName="stra_na_troska_sem_videl__4IZz" Text="Viděl sem zas Andulku. Co tady posluhuje..." />
									<Commands>
										<AnimationCommand Delay="-0.25" FragmentId="ADLG_Gesture" Guid="f897a1b3-c338-416e-88ea-7345a13432a2" Variant="26" DesiredDuration="1.9" />
									</Commands>
								</Response>
								<Response Role="NA_TROSKACH_TLACHAC_STRAZ_2" ReferenceLength="2.29">
									<Text StringName="stra_na_troska_no_kdo_by__eL1t" Text="No, kdo by ji přehlídl, že jo... " />
									<Commands>
										<AnimationCommand Delay="0.52" FragmentId="ADLG_Gesture" Guid="d85ed9b0-40b0-4887-a715-debff5158150" Variant="21" DesiredDuration="2.16" />
									</Commands>
								</Response>
								<Response Role="NA_TROSKACH_PACHOLEK" ReferenceLength="3.2">
									<Text StringName="stra_na_troska_prave_z_ml_SEcV" Text="Právě. Ze mlejna je to sice pěkná štreka a cesty pekně na hovno..." />
									<Commands>
										<AnimationCommand Delay="0.01" FragmentId="ADLG_Nod" Guid="ca364fe9-66bd-48fa-8178-0e49459d7038" Variant="2" DesiredDuration="1.25" />
										<AnimationCommand Delay="3.02" FragmentId="ADLG_Gesture" Guid="649b3c66-46dc-4666-8a6c-7d8df90c80ea" Variant="10" DesiredDuration="1.94" />
									</Commands>
								</Response>
								<Response Role="NA_TROSKACH_PACHOLEK" ReferenceLength="1.55">
									<Text StringName="stra_na_troska_ale_kvuli__k3ri" Text="Ale kvůli Andulce... Kvůli ní sem dycky měl radost, že sem pojedu." />
									<Commands>
										<AnimationCommand Role="NA_TROSKACH_TLACHAC_STRAZ_2" Delay="1.52" FragmentId="ADLG_Confusion" Guid="19e1b926-8753-4893-af45-10d39fa3a985" Variant="0" DesiredDuration="2.26" />
									</Commands>
								</Response>
								<Response Role="NA_TROSKACH_TLACHAC_STRAZ_2">
									<Text StringName="stra_na_troska_to_mi_povi_uJOn" Text="To mi povídej... Taky mě nenechala chladnym." />
								</Response>
								<Response Role="NA_TROSKACH_PACHOLEK" ReferenceLength="2.3">
									<Text StringName="stra_na_troska_tak_to_sis_AXg2" Text="Tak to sis asi všimnul toho co já, co?" />
									<Commands>
										<AnimationCommand Delay="0.01" FragmentId="ADLG_Confusion" Guid="5da7fd8b-7cec-40c8-a723-f3cc47022f36" Variant="4" DesiredDuration="2.16" />
										<AnimationCommand Role="NA_TROSKACH_TLACHAC_STRAZ_2" Delay="2.14" FragmentId="ADLG_Gesture" Guid="82614ed5-8978-4c2d-8771-aeab0b05a4e7" Variant="14" DesiredDuration="2.06" />
									</Commands>
								</Response>
								<Response Role="NA_TROSKACH_TLACHAC_STRAZ_2" ReferenceLength="3.84">
									<Text StringName="stra_na_troska_chlapi_nem_AF6C" Text="Chlapi nemluvěj pomalu vo ničem jinym. A svatba žádná nebyla..." />
									<Commands>
										<AnimationCommand Role="NA_TROSKACH_PACHOLEK" Delay="3.66" FragmentId="ADLG_Surprised_light" Guid="078c2b11-e2e3-4f4c-baf5-92aa3590fa78" Variant="1" DesiredDuration="3.36" />
									</Commands>
								</Response>
								<Response Role="NA_TROSKACH_PACHOLEK" ReferenceLength="1.67">
									<Text StringName="stra_na_troska_no_a_vi_se_AmDO" Text="No a ví se, kdo jí to udělal?" />
									<Commands>
										<AnimationCommand Role="NA_TROSKACH_TLACHAC_STRAZ_2" Delay="1.55" FragmentId="ADLG_Disagree_light" Guid="231a82ae-beac-4474-8538-e58365c056b8" Variant="0" DesiredDuration="1.18" />
									</Commands>
								</Response>
								<Response Role="NA_TROSKACH_TLACHAC_STRAZ_2" ReferenceLength="4.52">
									<Text StringName="stra_na_troska_to_je_prav_HhW5" Text="Neví. Její fotr samozřejmě šílí, ona pořád brečí..." />
									<Commands>
										<AnimationCommand Role="NA_TROSKACH_PACHOLEK" Delay="4.35" FragmentId="ADLG_Frustration" Guid="66924018-bb87-46e7-b19a-08a3f28cb3ef" Variant="2" DesiredDuration="2.68" />
									</Commands>
								</Response>
								<Response Role="NA_TROSKACH_PACHOLEK">
									<Text StringName="stra_na_troska_takze_se_k_NJst" Text="Takže se k tomu nikdo nemá?" />
								</Response>
								<Response Role="NA_TROSKACH_TLACHAC_STRAZ_2" ReferenceLength="2.09">
									<Text StringName="stra_na_troska_nikdo_EJ3z" Text="Nikdo." />
									<Commands>
										<AnimationCommand Delay="0.11" FragmentId="ADLG_Disagree_light" Guid="2fe9e83f-b40b-4748-b0ac-1e2f5d132e45" Variant="1" DesiredDuration="1.2" />
