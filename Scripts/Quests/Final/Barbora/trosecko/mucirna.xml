			<Objectives>
				<Objective TypeT="AdditiveLog" Name="mucirna_a">
					<LocalizedName StringName="mucirna_muceni_UaIr" Text="Výslech v mučírně" />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<Log Name="DefaultLog">
							<Log StringName="muceni_pan_bergov_nas_s_p_Pv3o" Text="Pan Bergov nás s Ptáčkem pověřil výslechem zajatce, kterýho se jim podařilo chytit během přepadení jeho družiny. Vojáci se z něj už snažili něco dostat, ale bezúspěšně." />
						</Log>
					</Logs>
				</Objective>
				<Objective TypeT="Progress" Name="promluv_si_s_ptackem">
					<LocalizedName StringName="mucirna_promluv_si_s_ptac_Lhiq" Text="Promluv si s Ptáčkem." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="ptacek">
							<Log StringName="promluv_s_jeste_nez_zacne_LbGC" Text="Nabídli jsme se Bergovovi, že zkusíme vyslechnout vězně, ale ještě, než začneme, bylo by dobré se o tom s Ptáčkem pobavit." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done" />
					</Logs>
				</Objective>
				<Objective TypeT="TorturingPrisoner" Name="vyslechni_s_ptackem_zajatce_v_mucirne">
					<LocalizedName StringName="semin_vyslechni_v_mucirne_Wu0m" Text="Vyslechni s Ptáčkem zajatce v mučírně." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="EnterTheTortureChamber" Marker="tortureChamberDoor">
							<Log StringName="vyslechni_placeholder_ent_CFeT" Text="Nezbývá než vstoupit do mučírny..." />
						</EnumLog>
						<EnumLog Type="None" Name="CaponWaitedForTooLongHidden" />
						<EnumLog Type="Updated" Name="CaponWaitedForTooLong" Marker="ptacek">
							<Log StringName="vyslechni_placeholder_cap_F6CX" Text="Nechtělo se mi do mučírny hned a Ptáček na mě přestal čekat. Musím ho ale najít a domluvit se s ním, ať tam jdeme společně." />
						</EnumLog>
						<EnumLog Type="Completed" Name="TorturingDone">
							<Log StringName="vyslechni_od_zajatce_v_mu_H5cB" Text="Od zajatce v mučírně se nám podařilo zjistit několik informací, který by pana Bergova mohly zajímat. Uvidíme, co na to řekne." />
						</EnumLog>
						<Log Name="CaptiveDiedLog">
							<Log StringName="vyslechni_asi_se_mu_ale_n_Seo7" Text="Asi se mu ale nebude líbit, že jeho zajatec výslech nevydržel." />
						</Log>
						<Log Name="HenryKnowsAboutSemin">
							<Log StringName="vyslechni_s_lapky_pracuje_I08y" Text="S lapky pracuje syn pana Semína, Oldřich!" />
						</Log>
						<Log Name="HenryKnowsAboutNebakov">
							<Log StringName="vyslechni_sidlo_lapku_je__RLmB" Text="Sídlo lapků je na Nebákově. Podařilo se jim přemoct a zajmnout tamnějšího pána." />
						</Log>
					</Logs>
				</Objective>
				<Objective TypeT="bergovDebriefObjective" Name="sdel_bergovovi_vysledek_vyslechu">
					<LocalizedName StringName="semin_vrat_se_k_bergovovi_pKLz" Text="Sděl Bergovovi výsledek výslechu." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="bergov">
							<Log StringName="vrat_se_k_meli_bychom_zaj_WQtw" Text="Měli bychom zajít za panem Bergovem a říct mu, jak dopadlo mučení... Mám pocit, že to, co prozradíme, může rozhodnout o osudu spousty lidí." />
						</EnumLog>
						<EnumLog Type="Completed" Name="DoneRaidToSeminGranted">
							<Log StringName="vrat_se_k_s_panem_bergove_ncvD" Text="S panem Bergovem jsme probrali všechny informace a je rozhodnuto - máme s Ptáčkem, purkrabím Haškem a několika jeho muži vyrazit na semínskou tvrz." />
						</EnumLog>
						<EnumLog Type="Completed" Name="DoneRaidToSeminRefused">
							<Log StringName="sdel_berg_podarilo_se_nam_Ve3Q" Text="Podařilo se nám přesvědčit pana Bergova, že Oldřich Semín je v přepadení jeho skupiny nevinně a možná jsme tak zabránili krveprolití. Teď je v plánu útok na lapky sídlící na Nebákově - což nám pan Bergov víc osvětlí na večerní hostině." />
						</EnumLog>
					</Logs>
				</Objective>
				<Objective TypeT="Progress" Name="pockej_na_podvecer_nez_hasek_pripravi_svoje_muze" IsOptional="true">
					<LocalizedName StringName="semin_novypockej_do_vecer_iN0y" Text="Počkej, než Hašek připraví svoje muže." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active">
							<Log StringName="novypocke_purkrabi_hasek__jfY6" Text="Purkrabí Hašek potřebuje trochu času na přípravu své družiny kvůli výpravě na Semín. Máme tak s Ptáčkem chvíli na vlastní přípravy, ale neměl bych se moc vzdalovat od Trosek, abych nepropásl odjezd. Možná bych mohl zkusit promluvit s Ptáčkem, třeba se mu nebude chtít čekat." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="novypocke_hasek_pripravil_aTu8" Text="Hašek připravil svoji družinu k odjezdu na Semín. Měli bychom se k němu s Ptáčkem přidat." />
						</EnumLog>
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="promluv_si_s_ptackem_kvuli_seminu">
					<LocalizedName StringName="semin_novypromluv_si_s_pt_dGyI" Text="Promluv si s Ptáčkem kvůli Semínu." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="ptacek">
							<Log StringName="novyproml_mel_bych_zajit__TKP3" Text="Měl bych se s Ptáčkem domluvit ohledně odjezdu na Semín. Raděj bych se neměl nikde moc zdržovat, Hašek působil, že je mu docela jedno, jestli se k němu přidáme nebo ne. " />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="novyproml_setkali_jsme_se_JTwS" Text="Vyrazili jsme vstříc Semínu." />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled">
							<Log StringName="bez_na_na__placeholder_pr_8njF" Text="Nestihl jsem Haška s Ptáčkem před odjezdem na Semín! Teď za nima můžu tak akorát vyrazit sám a doufat, že je doženu." />
						</EnumLog>
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="dozen_vypravu_mirici_na_semin">
					<LocalizedName StringName="mucirna_placeholder_dozen_3TgV" Text="Dožeň výpravu mířící na Semín." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="hetman">
							<Log StringName="placehold_placeholder_mus_KtcT" Text="Otálel jsem a vojáci vyjeli beze mě. Musím je dohnat!" />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="placehold_placeholder_doh_fCCf" Text="Stihl jsem se připojit k družině, která míří na Semín..." />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled">
							<Log StringName="placehold_placeholder_nes_THpN" Text="Vojáci odjeli beze mě a nevím, jestli se mi podaří dostat na Semín... Spíš ne." />
						</EnumLog>
					</Logs>
				</Objective>
				<Objective TypeT="ProgressFailable" Name="jed_s_vojaky_a_ptackem_na_semin">
					<LocalizedName StringName="semin_jed_s_vojaky_a_ptac_UBk9" Text="Jeď s vojáky a Ptáčkem na Semín." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="hetman">
							<Log StringName="jed_s_voj_vyrazili_jsme_s_P1XT" Text="Vyrazili jsme s Ptáčkem, purkrabím a jeho družinou na Semín. Je to dlouhá cesta, ale aspoň bude příležitost k hovoru. Docela mě zajímají detaily z toho přepadení. Přece jen - taky jsme si tím prošli." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="jed_s_voj_dorazili_jsme_s_QEWb" Text="Dorazili jsme s celou družinou na Semín, po cestě se k nám ještě přidali další vojáci. Uvidíme, jak se bude situace vyvíjet, ale nemám z toho dobrý pocit." />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled">
							<Log StringName="jed_s_voj_nestihl_jsem_s__g6RD" Text="Nestihl jsem s Ptáčkem a ostatníma dorazit na Semín a teď už se přes zavřenu bránu nedostanu. Nezbyvá mi nic jinýho, než se Ptáčka po návratu na všechno zeptat." />
						</EnumLog>
					</Logs>
				</Objective>
				<Objective TypeT="ProgressAbortable" Name="poraz_semina_a_jeho_muze">
					<LocalizedName StringName="semin_pozabijej_seminske_8ODz" Text="Poraž Semína a jeho muže!" />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Started">
							<Log StringName="pozabijej_klidnej_rozhovo_ttyz" Text="Klidnej rozhovor se rychle vyhrotil do brutální bitvy! Musíme se bránit a nastolit v tvrzi pořádek, jinak se vodtud živí nedostanem! Ale... jsou to přece jen obyčejní lidi?!" />
						</EnumLog>
						<EnumLog Type="Completed" Name="Completed">
							<Log StringName="pozabijej_po_pzrijezdu_na_V0Iv" Text="Po příjezdu na Semín jsme se snažili situaci vyřešit ze začátku klidně, ale purkrabí ryhle přešel k vyhrožování. Než jsme se nadáli, začala nepřehledná a drsná bitka. Bergovovi muži se chovali vůči civilistům dost brutálně - ale, jsme ve válce. Asi." />
						</EnumLog>
						<EnumLog Type="None" Name="Aborted">
							<Log StringName="pozabijej_nemohl_jsem_nec_m1cm" Text="Nemohl jsem nečinně přihlížet masakru semínských a zaútočil na vojáky... Tohle se Bergov nesmí dozvedět." />
						</EnumLog>
					</Logs>
				</Objective>
				<Objective TypeT="KillSoldiers" Name="pozabijej_vojaky">
					<LocalizedName StringName="semin_pozabijej_vojaky_pmvK" Text="Pozabíjej vojáky!" />
					<Logs>
