<?xml version="1.0" encoding="utf-8"?>
<Database xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="brambora">
	<Skald>
		<Scene Name="cin_s1450t_hledanipsa__henrys_dog" IsLocked="true">
			<Text StringName="cin_s1450t_hledanipsa__he_auAg" Text="Hráče se zastává jeho pes" />
			<Elements>
				<SceneActionElement>
					<Text StringName="cin_s1450_henry_se_prodir_e0nZ" Text="Henry se prodírá křovím na na mýtinu, možná to vypadá, že sledoval stopy, ale možná taky ne. Zničeho nic se ozve zavytí." />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_henry_sebou_trh_XLWu" Text="Henry sebou trhne a zahledí se směrem, odkud se ten zvuk ozval. Na skále stojí Vořech, zírá na hráče a znovu zavyje." />
				</SceneActionElement>
				<LinearDialogueElement>
					<Dialogue AllowGreeting="false">
						<Decision Name="dec1" Priority="SideQuestSubmission">
							<Sequences>
								<Sequence Name="seq1">
									<DesignName StringName="cin_s1450_seq1_lmWK" Text="seq1" />
									<Elements>
										<Response Role="HENRY">
											<Parenthetical>
												<Text StringName="cin__henry_vahave_opatrne_abnC" Text="váhavě, opatrně" />
											</Parenthetical>
											<Text StringName="cin__henry_vorechu_eu8F" Text="Vořechu?" />
										</Response>
									</Elements>
								</Sequence>
							</Sequences>
						</Decision>
					</Dialogue>
				</LinearDialogueElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_usmeje_se_a_vyd_QLOh" Text="Usměje se a vydá se směrem k němu." />
				</SceneActionElement>
				<LinearDialogueElement>
					<Dialogue>
						<Decision Name="dec1" Priority="SideQuestSubmission">
							<Sequences>
								<Sequence Name="seq1">
									<DesignName StringName="cin_s1450_seq1_8Prg" Text="seq1" />
									<Elements>
										<Response Role="HENRY">
											<Parenthetical>
												<Text StringName="cin__henry_stastne_ypx3" Text="šťastně" />
											</Parenthetical>
											<Text StringName="cin__henry_vorechu_pod_se_HKtY" Text="Vořechu! Poď sem! No hodnej, hodnej!" />
										</Response>
									</Elements>
								</Sequence>
							</Sequences>
						</Decision>
					</Dialogue>
				</LinearDialogueElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_henry_se_vyda_s_7p0a" Text="Ozve se zavrčení. Vlci... " />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_vorech_seskoci__xWrk" Text="Smečka obestupuje Henryho. Do popředí vystoupí velký zlý čoklovlk a Zavrčí na henryho. Henry jednu ruku položí na hlavici sekery za pasem, druhá je pořád natažená kupředu. Ucouvne o krok dozadu, velký zlý pes ho o krok následuje smečka stojí na místě." />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_velky_zly_pes_m_nfId" Text="Velký zlý pes mu v odpověď zavrčí. Smečka za nima se trochu rozestoupí, jak se mezi nima protáhne Vořech, který se taky pomalu začne přibližovat k Henrymu. VElký zlý vlk zavrčí na Vořecha, který se od něj začne vzdalovat a tím obcházet henryho do boku. Henry se natočí víc proti Vořechovi ale pořád podezřívavě pokukuje po velkém zlém psovi." />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_vorech_se_na_ta_mMn6" Text="Vořech se na ta slova zarazí, udělá krok k henrymu, natáhne trochu krk a začichá.  Pak zavrčí, ale ne tak silně jako velký zlý pes." />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_vorech_se_zacne_3ozU" Text="Vořech se začne přibližovat a vrčet na Henryho." />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_vorech_se_dal_p_D904" Text="Vořech se dál přibližuje, smečka za ním se pomalu taky začne pohybovat a rozprostírat. Vořech překoná hranici odkud by na Henryho skočil a ještě se trochu přiblíží." />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_vorech_udela_je_xi0m" Text="Vořech udělá ještě krok, vycení zuby a vyplázne jazyk." />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_vorech_je_ted_t_okRW" Text="Vořech je teď tak blízko hráči, že se snadno může postavit proti hlavnímu zlému vlkovi, který se celou dobu nelibě šklebil, kdo že se mu to sere do vedení." />
				</SceneActionElement>
				<LinearDialogueElement>
					<Dialogue>
						<Decision Name="dec1" Priority="SideQuestSubmission">
							<Sequences>
								<Sequence Name="seq1">
									<DesignName StringName="cin_s1450_seq1_RMkU" Text="seq1" />
									<Elements>
										<Response Role="HENRY">
											<Parenthetical>
												<Text StringName="cin__henry_k_vorechovi_ul0W" Text="k Vořechovi" />
											</Parenthetical>
											<Text StringName="cin__henry_trhej_1iQT" Text="Trhej!" />
										</Response>
									</Elements>
								</Sequence>
							</Sequences>
						</Decision>
					</Dialogue>
				</LinearDialogueElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_oba_dva_na_sebe_YPFe" Text="Oba dva na sebe ošklivě zavrčí a vyjedou po sobě. Jindra tasí." />
				</SceneActionElement>
				<SceneActionElement>
					<Text StringName="cin_s1450_end_NS4n" Text="End." />
				</SceneActionElement>
			</Elements>
		</Scene>
	</Skald>
