			<Text StringName="mq09__utok_na_nebakov_7QLD" Text="Útok na Nebákov" />
			<MapVizLocation MapName="trosecko" PositionX="2043" PositionY="1447" />
			<QuestName StringName="qname_utoknanebakov_rrcD" Text="Za vítězství!" />
			<QuestDescription StringName="qdesc_utoknanebakov_vHSU" Text="Konečně nadešel čas skoncovat s těmi lotry, kteří nás přepadli u rybníka a zabili naše přátele. Již brzy proto zaútočíme na jejich doupě a zajistíme, aby nikdo z těch zlosynů neunikl spravedlnosti!" />
			<Objectives>
				<Objective TypeT="Progress" Name="promluv_si_vecer_s_ptackem">
					<LocalizedName StringName="utoknaneb_promluv_si_s_pt_hWlM" Text="Promluv si večer s Ptáčkem." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="ptacek">
							<Log StringName="promluv_s_za_nejaky_cas_s_ajLq" Text="Večer se mám zastavit za Ptáčkem. Pan Bergov mu opět svěřil jistý zvláštní úkol, ale tentokrát na to jistě nebudeme sami. Chystá se něco většího, protože pán z Bergova pozval na hrad své šlechtické vazaly i s jejich zbrojnoši." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="promluv_s_ptacek_mi_prozr_c4S2" Text="Ptáček mi prozradil, že pan Ota z Bergova nás pozval na večerní hostinu. Slavnost byla na počest jeho vazalů, kteří příští ráno měli vyrazit do útoku proti hradu Nebákov. Nyní totiž bylo jisté, že banda, která nás přepadla u rybníka a škodila po kraji, obsadila tento skalní hrad a jeho pána Jaromíra držela v zajetí." />
						</EnumLog>
					</Logs>
				</Objective>
				<Objective TypeT="ProgressAbortable" Name="hoduj_a_uzivej_si_zivota" IsOptional="true">
					<LocalizedName StringName="utoknaneb_pij_hoduj_a_uzi_qoMg" Text="Hoduj a užívej si života!" />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Started" Marker="feast_areaMarker">
							<Log StringName="pij_hoduj_hostina_je_v_pl_pakZ" Text="Hostina je v plném proudu, byla by škoda se trochu nepobavit. Ostatně: zítra jdeme do bitvy a člověk nikdy neví, zda tahle číše vína nebo ta chutná pečínka nejsou jeho poslední. Je to také příležitost jak se pobavit s urozenou společností a možná získat nové přátele. " />
						</EnumLog>
						<EnumLog Type="Completed" Name="Completed">
							<Log StringName="pij_hoduj_hostina_na_tros_rKG1" Text="Hostina na Troskách se protáhla do noci. Po všem tom strádání jsme konečně mohli hodit starosti za hlavu. Velení útoku dostal na starost komoří Ulrich von Schaumberg a od nás s Ptáčkem se nic zvláštního nečekalo, kromě toho že se tažení zúčastníme." />
						</EnumLog>
						<EnumLog Type="None" Name="Aborted" />
					</Logs>
				</Objective>
				<Objective TypeT="Progress" Name="dobre_se_vyspi">
					<LocalizedName StringName="utoknaneb_placeholder__jd_0bzm" Text="Dobře se vyspi." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="playersBed">
							<Log StringName="dobre_se__pred_bitvou_bud_22Qg" Text="Před bitvou bude dobré se trochu vyspat, a hlavně nezaspat." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
							<Log StringName="dobre_se__po_bujarem_vece_i8sF" Text="Bujaré veselí hradní hostiny přešlo v klidnou a tichou noc." />
						</EnumLog>
					</Logs>
				</Objective>
				<Objective TypeT="ProgressAbortable" Name="zajdi_za_ptackem">
					<LocalizedName StringName="utoknaneb_zajdi_za_ptackem_HIBd" Text="Zajdi za Ptáčkem." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Started" Marker="ptacek">
							<Log StringName="zajdi_za__jan_ptacek_vypa_Rdq4" Text="Jan Ptáček vypadá od rozhovoru s Otou z Bergova nějak sklesle. Konečně je na hostině mezi spoustou pěkných ženských a on stojí potichu v koutě. Zajdu za ním a zeptám se ho, o co jde." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Completed">
							<Log StringName="zajdi_za__jan_ptacek_nema_jbAB" Text="Jan Ptáček nemá rád, když mu kdokoliv něco přikazuje. Služba pánu z Bergova ho začínala pěkně otravovat. Zejména rozhovory s komořím Ulrichem mu nedělali zrovna dvakrát dobře. Ti dva si rozhodně nepadli do oka." />
						</EnumLog>
						<EnumLog Type="None" Name="Aborted" />
					</Logs>
				</Objective>
				<Objective TypeT="GetFridusRing" Name="prines_florianuv_prsten" IsOptional="true">
					<LocalizedName StringName="utoknaneb_ziskej_fridusuv_yQUw" Text="Přines Floriánův prsten." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Active" Marker="fridusStash">
							<Log StringName="ziskej_fr_herman_palecek__mkGp" Text="Heřman Paleček se chce pomstít Floriánovi za jeho poslední žert. Pomůže mu v tom  prsten, který rytíř Florián nedávno dostal od jedné dámy. Prsten je zamčený v jeho truhle ve věži Baba." />
						</EnumLog>
						<EnumLog Type="Updated" Name="Update" Marker="Herman">
							<Log StringName="ziskej_fr_ted_uz_staci_je_nIce" Text="Teď už stačí jen odnést Floriánův prsten zpátky Heřmanu Palečkovi." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Completed">
							<Log StringName="ziskej_fr_v_truhle_ve_vez_pMc1" Text="Ve Floriánově truhle ve věži Baba jsem našel stříbrný prsten a přinesl ho Palečkovi. Jeho intrika spočívala v šikovném podstrčení prstenu jiné dámě, aby Florián od své pravé ctitelky dostal pěkně za vyučenou." />
						</EnumLog>
						<EnumLog Type="Canceled" Name="Canceled">
							<Log StringName="ziskej_fr_herman_palecek__eA8K" Text="Rytíř Heřman Paleček se chtěl na hostině pomstít svému sokovi Floriánovy, ale jak to dopadlo jsem se už nedozvěděl." />
						</EnumLog>
						<EnumLog Type="None" Name="Confiscated" />
					</Logs>
				</Objective>
				<Objective TypeT="ProgressAbortable" Name="proved_intriku_s_prstenem" IsOptional="true">
					<LocalizedName StringName="utoknaneb_proved_intriku__CJdD" Text="Proveď intriku s prstenem." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Started" Marker="Adleta">
							<Log StringName="proved_in_rekl_jsem_heram_jFoZ" Text="Řekl jsem Palečkovi, že jeho intriku s prstenem provedu za něj. Byl jsem zvědavý, co se stane." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Completed" />
						<EnumLog Type="None" Name="Aborted" />
						<Log Name="IntrikaSuccess">
							<Log StringName="proved_in_intrika_s_prste_BQ7G" Text="Intrika s prstenem vyšla přesně jak Heřman naplánoval. Floriánova tajná ctitelka prsten poznala a zbytek už zařídily city a její přímočarost." />
						</Log>
						<Log Name="IntrikaNotGood">
							<Log StringName="proved_in_intrika_s_prste_sTQl" Text="Intrika s prstenem nevyšla. Špatně jsem odhadl rytíře Floriána a jeho dámy." />
						</Log>
					</Logs>
				</Objective>
				<Objective TypeT="ProgressAbortable" Name="ziskej_odmenu_od_palecka">
					<LocalizedName StringName="utoknaneb_zajdi_si_k_pale_hYEN" Text="Získej odměnu od Palečka." />
					<Logs>
						<EnumLog Type="None" Name="None" />
						<EnumLog Type="Started" Name="Started" Marker="Herman">
							<Log StringName="zajdi_si__palecek_me_slib_2GVz" Text="Heřman Paleček mi slíbil odměnu za provedení intriky s Floriánovým prstenem." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Completed">
							<Log StringName="zajdi_si__palecek_se_mi_o_m28r" Text="Později se mi Heřman Paleček dobře odvděčil. Navíc jsme pochopil, že svět rytířů a ctěných dam má svá nepsaná pravidla a stinná zákoutí." />
						</EnumLog>
						<EnumLog Type="None" Name="Aborted" />
					</Logs>
