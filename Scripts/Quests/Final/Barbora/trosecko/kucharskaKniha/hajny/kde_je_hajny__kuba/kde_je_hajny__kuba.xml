												<Text StringName="kde__henry_to_udelam_diky_XSZ3" Text="To udělám. Díky za radu." />
												<Commands>
													<CameraCommand />
												</Commands>
											</Response>
											<Response Role="KUBA">
												<Text StringName="kde__kuba_neni_zac_pane_ADog" Text="Není zač, pane." />
