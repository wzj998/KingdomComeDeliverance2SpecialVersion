										</Triggers>
										<Elements>
											<Response Role="HENRY">
												<Text StringName="kde__henry_anci_uz_jsem_s_tDnZ" Text="Anči už jsem se ptal. Ale to s tou skříní mi teda neporadila." />
												<Commands>
													<CameraCommand />
												</Commands>
											</Response>
										</Elements>
									</Sequence>
