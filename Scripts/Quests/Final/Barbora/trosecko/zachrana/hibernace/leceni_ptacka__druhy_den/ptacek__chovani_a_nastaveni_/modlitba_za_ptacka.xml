	<Skald>
		<Dialog Name="modlitba_za_ptacka">
			<Text StringName="modlitba_za_ptacka_TrO6" Text="Modlitba za Ptáčka" />
			<Dialogue Type="ingame monolog" TechnicalStatus="Enabled" Initiator="NonPlayer" GesturesNotNeeded="true">
				<Decision Name="dec1" Priority="MainQuestSubmission" Alias="zachrana_modlitbaZaPtacka">
					<Sequences>
						<Sequence EndType="EndDialogue" Name="seq1">
							<DesignName StringName="modlitba__seq1_XDjw" Text="seq1" />
							<Elements>
								<Response Role="HENRY">
									<Text StringName="modl_henry_hahahaha_P2Op" Text="Milostivej Pane náš všemohoucí." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_hahahahaha_iwWl" Text="Shlídni prosím na mě na hříšníka a vyslyš mojí modlitbu upřímnou." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_hahahaha_zllC" Text="Ochraňuj prosím pana Ptáčka a nedej mu zahynout, tak jako on nedal zahynout mě." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_zachovej_mu_zi_5SKG" Text="Zachovej mu život, protože je to pán mladej, statečnej a spravedlivej a dobrej křesťan jak se sluší." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_a_shlidni_pros_MPZ1" Text="A shlídni prosím i na Boženu a dej jí, ať Ptáčka zachrání." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_a_od_vseho_zly_ywdi" Text="A od všeho zlýho jí ochraňuj a dopřej jí milosti svojí, protože je sama milosrdná a pomoc nám v nouzi neodepřela." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_amen_j9EN" Text="Amen." />
								</Response>
							</Elements>
						</Sequence>
					</Sequences>
				</Decision>
			</Dialogue>
		</Dialog>
	</Skald>
