				<Decision Name="dec1" Priority="MainQuestSubmission" Alias="zachrana_bozenaAPavlenaSeBaviUSnidanePoTetralogu">
					<Sequences>
						<Sequence EndType="EndDialogue" Name="seq1">
							<DesignName StringName="bozena_a__seq1_3KG9" Text="seq1" />
							<Elements>
								<Response Role="PAVLENA_DCERA_KORENARKY" ReferenceLength="3.48">
									<Text StringName="boze_pavlena_d_ss_mIjC" Text="Promiň mami, že jsem přišla pozdě, zdržela jsem se v Troskovicích." />
									<Commands>
										<MoodCommand Mood="sittingAtTable" />
										<MoodCommand Role="BOZENA_KORENARKA" Mood="sittingAtTable" />
										<AnimationCommand Delay="2.15" FragmentId="ADLG_Little_Gest" Guid="ca31f541-eb00-4551-85b4-fff798ed9d8a" Variant="2" />
									</Commands>
								</Response>
								<Response Role="BOZENA_KORENARKA" ReferenceLength="3.96">
									<Parenthetical>
										<Text StringName="boze_bozena_ko_nelibe_KKfl" Text="nelibě" />
									</Parenthetical>
									<Text StringName="boze_bozena_ko_ze_zrovna__GZF6" Text="Že zrovna tam. A nebylo to poprvý." />
									<Commands>
										<AnimationCommand Delay="2.49" FragmentId="ADLG_Discard" Guid="3fbdaa4b-6f68-4443-b500-2b499ec2c660" Variant="0" />
									</Commands>
								</Response>
								<Response Role="PAVLENA_DCERA_KORENARKY" ReferenceLength="6.29">
									<Text StringName="boze_pavlena_d_vim_ale_me_sVgU" Text="Vim, měla jsem cestu kolem, tak jsem ještě zkusila udat nějaký košíky a mastě. A povedlo se." />
									<Commands>
										<AnimationCommand Delay="3.29" FragmentId="ADLG_Little_Gest" Guid="97737080-d737-4074-85cc-0e3642e002d5" Variant="1" />
									</Commands>
								</Response>
								<Response Role="BOZENA_KORENARKA">
									<Text StringName="boze_bozena_ko_emmerich_n_LeMR" Text="Emmerich nebude rád." />
								</Response>
								<Response Role="PAVLENA_DCERA_KORENARKY" ReferenceLength="7.2">
									<Text StringName="boze_pavlena_d_tak_nema_d_yNzK" Text="Tak nemá dávat tak draho. Často chce skoro dvakrát tolik co my, tak ať se nediví, že lidi berou od nás." />
									<Commands>
										<AnimationCommand Delay="1.55" FragmentId="ADLG_Little_Gest" Guid="e309cd9f-9069-439b-a356-a0eedc26c15a" Variant="3" />
									</Commands>
								</Response>
								<Response Role="PAVLENA_DCERA_KORENARKY">
									<Text StringName="boze_pavlena_d_ale_rozumi_Q2RH" Text="Ale rozumim. Příště už půjdu rovnou domu, ať seš tu sama co nejmíň." />
								</Response>
								<Response Role="PAVLENA_DCERA_KORENARKY" ReferenceLength="6.48">
									<Text StringName="boze_pavlena_d_jestli_ti__zX0i" Text="Jestli ti sem zas někdo přijde vyhrožovat, počastuju ho šípem. To je naučí si na nás dovolovat." />
									<Commands>
										<AnimationCommand Delay="0.62" FragmentId="ADLG_Little_Gest" Guid="7573e3c3-3f1e-45b6-a2cc-e1c73b854c99" Variant="0" />
									</Commands>
								</Response>
								<Response Role="BOZENA_KORENARKA">
									<Text StringName="boze_bozena_ko_no_a_nebo__3T2V" Text="Možná dobře, žes tu nebyla. Ještě by tě chytili a víc se nechci domejšlet." />
								</Response>
								<Response Role="PAVLENA_DCERA_KORENARKY" ReferenceLength="5.53">
									<Text StringName="boze_pavlena_d_ne_vsak_vi_EfOr" Text="Nechytili. Však víš, že jsem dobrá. Když trefim ušáka za běhu, chlapa bych zvládla jak nic." />
									<Commands>
										<AnimationCommand Delay="0.03" FragmentId="ADLG_Discard" Guid="3fbdaa4b-6f68-4443-b500-2b499ec2c660" Variant="0" />
									</Commands>
								</Response>
								<Response Role="BOZENA_KORENARKA" ReferenceLength="12.08">
									<Text StringName="boze_bozena_ko_ok_ISH1" Text="To máš pravdu, ruku máš pevnou a oko ostrý, ne jak já. Ale víš jak to je. Čím míň se toho kolem nás semele, tím líp." />
									<Commands>
										<AnimationCommand Delay="2.68" FragmentId="ADLG_Little_Gest" Guid="ca31f541-eb00-4551-85b4-fff798ed9d8a" Variant="2" />
									</Commands>
								</Response>
								<Response Role="BOZENA_KORENARKA" ReferenceLength="6.46">
									<Text StringName="boze_bozena_ko_a_kdyby_pr_SPUC" Text="A kdyby přece něco, máme Bručouna a ten nás ve štychu nenechá." />
									<Commands>
										<AnimationCommand Delay="5.42" FragmentId="ADLG_Discard" Guid="3fbdaa4b-6f68-4443-b500-2b499ec2c660" Variant="0" />
									</Commands>
								</Response>
								<Response Role="PAVLENA_DCERA_KORENARKY" ReferenceLength="2.81">
									<Text StringName="boze_pavlena_d_ale_dobre__Tv8K" Text="To jo, je to zlatý zvíře." />
									<Commands>
										<AnimationCommand Delay="1.09" FragmentId="ADLG_Little_Gest" Guid="97737080-d737-4074-85cc-0e3642e002d5" Variant="1" />
									</Commands>
								</Response>
								<Response Role="PAVLENA_DCERA_KORENARKY" StartDelay="0.32">
									<Text StringName="boze_pavlena_d_cas_udelat_tBqL" Text="Čas udělat něco užitečnýho, dny jsou sice dlouhý, ale práce je nad hlavu." />
								</Response>
								<Response Role="BOZENA_KORENARKA" ReferenceLength="5.12">
									<Text StringName="boze_bozena_ko_tak_tak_de_tTux" Text="Tak tak, děkuju Pavlenko. Bez tebe nevím co bych si tu počala." />
									<Commands>
										<AnimationCommand Delay="2.58" FragmentId="ADLG_Little_Gest" Guid="e309cd9f-9069-439b-a356-a0eedc26c15a" Variant="3" />
									</Commands>
								</Response>
