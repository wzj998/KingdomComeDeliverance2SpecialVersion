<?xml version="1.0" encoding="utf-8"?>
<Database xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="brambora">
	<Skald>
		<FaderDialog Name="kocour_zbytky">
			<Text StringName="kocour_zbytky_qEsT" Text="Kocour_zbytky" />
			<Dialogue LocalizationStatus="DontTranslate">
				<Decision Name="dec1">
					<Sequences>
						<Sequence Name="seq1">
							<DesignName StringName="kocour_zb_seq1_L2KE" Text="seq1" />
							<UiPrompt StringName="ui_kocour_zb_seq1_tToF" Text="Kdo je Kocour?" />
							<Elements>
								<Response Role="HENRY">
									<Parenthetical>
										<Text StringName="koco_henry_zvidave_8xHi" Text="zvídavě" />
									</Parenthetical>
									<Text StringName="koco_henry_kdo_je_kocour_GaCy" Text="Kdo je Kocour?" />
									<Commands>
										<CameraCommand />
									</Commands>
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Parenthetical>
										<Text StringName="koco_hospodska_ukaze_U2PW" Text="ukáže" />
									</Parenthetical>
									<Text StringName="koco_hospodska_taky_jeden_1brD" Text="Taky jeden pocestnej, jako vy, ale nějak se mu tady v kraji zalíbilo." />
									<Commands>
										<CameraCommand />
									</Commands>
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Parenthetical>
										<Text StringName="koco_hospodska_unesene_eoqk" Text="unešeně" />
									</Parenthetical>
									<Text StringName="koco_hospodska_je_to_sila_rcwl" Text="Je to silák, lamželezo... Umí vohejbat meče jako by byly ze slámy!" />
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Text StringName="koco_hospodska_a_na_vlast_t0v0" Text="A na vlastní voči jsem viděla, jak se nechal přejet po vršek naloženým vozem a nic mu nebylo!" />
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Text StringName="koco_hospodska_predvadi_j_cZXY" Text="Předvádí, jak se bojuje s různejma zbraněma, polyká oheň..." />
								</Response>
								<Response Role="HENRY">
									<Parenthetical>
										<Text StringName="koco_henry_podivi_se_RerV" Text="podiví se" />
									</Parenthetical>
									<Text StringName="koco_henry_pani_kde_se_ta_59Gv" Text="Páni! Kde se tady takovej chlapík vzal?" />
									<Commands>
										<CameraCommand />
									</Commands>
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Text StringName="koco_hospodska_pritah_sem_yG3k" Text="Přitáh sem s těma kočovníkama, co se usídlili u Věžickýho rybníka. To je jinak pěkná sebranka." />
									<Commands>
										<CameraCommand />
									</Commands>
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Text StringName="koco_hospodska_s_kocourem_g6FF" Text="S Kocourem je aspoň zábava, vždycky když přijde." />
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Parenthetical>
										<Text StringName="koco_hospodska_trochu_sty_jV1l" Text="trochu stydlivě, tiše" />
									</Parenthetical>
									<Text StringName="koco_hospodska_a_navic_je_X5Jc" Text="A navíc... je to fešák." />
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Parenthetical>
										<Text StringName="koco_hospodska_usmeje_se_9JLF" Text="usměje se" />
									</Parenthetical>
									<Text StringName="koco_hospodska_ten_by_ty__TuG4" Text="Ten by ty mý pytle odnosil v zubech a ještě by u toho žongloval pochodněma! " />
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Parenthetical>
										<Text StringName="koco_hospodska_pokrci_ram_pINb" Text="pokrčí rameny" />
									</Parenthetical>
									<Text StringName="koco_hospodska_ale_ted_ta_oQRk" Text="Ale teď tady bohužel neni, takže je to na vás." />
								</Response>
							</Elements>
						</Sequence>
						<Sequence Name="seq44">
							<DesignName StringName="kocour_zb_seq44_su9e" Text="seq44" />
							<UiPrompt StringName="ui_kocour_zb_seq44_mOv0" Text="Cos říkala o Kocourovi?" />
							<Elements>
								<Response Role="HENRY">
									<Text StringName="koco_henry_cos_rikala_o_t_NpKz" Text="Cos říkala o tom Kocourovi? Bojovník, co žije s kočovníkama?" />
									<Commands>
										<CameraCommand />
									</Commands>
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Parenthetical>
										<Text StringName="koco_hospodska_usmeje_se_vyTI" Text="usměje se" />
									</Parenthetical>
									<Text StringName="koco_hospodska_jo_ten_chl_Fq6W" Text="Jo. Ten chlap umí snad všechno. Hlavně předvádí rozličný nebezpečný kousky, až to bere dech." />
									<Commands>
										<CameraCommand />
									</Commands>
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Text StringName="koco_hospodska_jeden_ze_s_JyLL" Text="Jeden ze strážnejch si s nim tuhle byl zašermovat v takový malý vohrádce, co si Kocour postavil v tom táboře." />
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Text StringName="koco_hospodska_prisel_pek_Fvl2" Text="Přišel pěkně zřízenej a ještě si pochvaloval, jak dostal do těla." />
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Parenthetical>
										<Text StringName="koco_hospodska_s_povzdech_M1sd" Text="s povzdechem" />
									</Parenthetical>
									<Text StringName="koco_hospodska_ja_vas_chl_TMnr" Text="Já vás chlapy někdy nechápu..." />
								</Response>
							</Elements>
						</Sequence>
						<Sequence EntryCondition="SequenceUsed('first_leaving')" Name="seq40">
							<DesignName StringName="kocour_zb_seq40_OEHO" Text="seq40" />
							<UiPrompt StringName="ui_kocour_zb_seq40_O6EB" Text="Cos říkala o kočovnících?" />
							<Elements>
								<Response Role="HENRY">
									<Text StringName="koco_henry_cos_rikala_o_t_Z2wI" Text="Cos říkala o těch kočovnících?" />
									<Commands>
										<CameraCommand />
									</Commands>
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Parenthetical>
										<Text StringName="koco_hospodska_prevraci_o_GDjP" Text="převrací očima" />
									</Parenthetical>
									<Text StringName="koco_hospodska_pritahli_b_VFWI" Text="Přitáhli Bůh ví odkud a utábořili se ve skalách u Věžickýho rybníka." />
									<Commands>
										<CameraCommand />
									</Commands>
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Text StringName="koco_hospodska_maj_ve_zvy_rNKz" Text="Maj ve zvyku se pořád dokola mlátit, střílet po sobě a šmelit s koňma. Žádná dobrá společnost pro bohabojnýho člověka." />
								</Response>
								<Response Role="HOSPODSKA_TROSKOVICE">
									<Text StringName="koco_hospodska_mluvej_nej_yJJp" Text="Mluvěj nějakou hatmatilkou a nic jim neni svatý. Vopatrně s nima." />
								</Response>
							</Elements>
						</Sequence>
					</Sequences>
				</Decision>
			</Dialogue>
		</FaderDialog>
	</Skald>
</Database>