							<DesignName StringName="sam_a_rab_seq1_wafd" Text="seq1" />
							<Elements>
								<Response Role="RABIN_POGROM" ReferenceLength="5.19">
									<Parenthetical>
										<Text StringName="sam__rabin_mluvi_tise_a_j_muxy" Text="mluví tiše a jidiš, jen mezi sebou" />
									</Parenthetical>
									<Text StringName="sam__rabin_to_jsem_rad_ze_7Uks" Text="Vos iz dajn plan? Vestu mit im curikgejn?">
										<Note StringName="sam__rabin_to_jsem_rad_ze_7Uks_note" Text="celé v jidiš: Co teď budeš dělat? Pojedeš s ním zpátky?" />
									</Text>
									<Commands>
										<FacialMoodCommand FacialMood="thinking1" />
										<AnimationCommand Delay="0" FragmentId="ADLG_Gesture" Guid="f897a1b3-c338-416e-88ea-7345a13432a2" Variant="26" />
										<AnimationCommand Role="SAMUEL_POGROM" Delay="4.97" FragmentId="ADLG_Gesture" Guid="0229b273-f5e4-4cb2-a60f-332f72774af3" Variant="1" />
									</Commands>
								</Response>
								<Response Role="SAMUEL_POGROM">
									<Parenthetical>
										<Text StringName="sam__samuel_take_mu_tise__AVJn" Text="také mu tiše odpovídá, je ale přímočarý a stojí si za svým" />
									</Parenthetical>
									<Text StringName="sam__samuel_ja_jsem_taky__0fDm" Text="Noch dem vos. Ich vel ajch kojdem in zicherkajt baglejtn.">
										<Note StringName="sam__samuel_ja_jsem_taky__0fDm_note" Text="celé v jidiš: To může počkat, nejdřív vás doprovodím do bezpečí." />
									</Text>
								</Response>
								<Response Role="RABIN_POGROM" ReferenceLength="10.78">
									<Text StringName="sam__rabin_ted_se_asi_puj_pNIk" Text="Ha-šem hot ništ unc hajtn geštroft, ba-avonousejnu ha-rabim. Anderš voltn mir ništ fun dem štot antrinen. ">
										<Note StringName="sam__rabin_ted_se_asi_puj_pNIk_note" Text="celé v jidiš: Kdyby nás pro naše četné hříchy chtěl Hospodin zabít, tak se ani nedosteneme z města." />
									</Text>
									<Commands>
										<AnimationCommand Delay="0.06" FragmentId="ADLG_Gesture" Guid="f7b6478f-50ab-4615-975b-b70e831408c4" Variant="25" />
										<AnimationCommand Delay="5.07" FragmentId="ADLG_Gesture" Guid="89606b07-8fa2-4d25-9b3d-da39e1476038" Variant="15" />
										<AnimationCommand Role="SAMUEL_POGROM" Delay="10.77" FragmentId="ADLG_Gesture" Guid="82614ed5-8978-4c2d-8771-aeab0b05a4e7" Variant="14" />
									</Commands>
								</Response>
								<Response Role="SAMUEL_POGROM" ReferenceLength="6.63">
									<Parenthetical>
										<Text StringName="sam__samuel_polozi_duraz__zdHW" Text="položí důraz na to já" />
									</Parenthetical>
									<Text StringName="sam__samuel_jak_bych_nemo_Ew4h" Text="Efšer, ober noch… Ich vel zajn rujker visndik, az du un Mame zajnen zicher.">
										<Note StringName="sam__samuel_jak_bych_nemo_Ew4h_note" Text="celé v jidiš: Možná, ale stejně... Já budu klidnější, když budu vědět, že jste s mámou v pořádku." />
									</Text>
									<Commands>
										<FacialMoodCommand FacialMood="thinking1" />
										<AnimationCommand Delay="4.69" FragmentId="ADLG_Gesture" Guid="34ac6648-6fd5-4005-84ec-632a56223892" Variant="5" />
										<AnimationCommand Role="RABIN_POGROM" Delay="6.59" FragmentId="ADLG_Nod" Guid="c05d1d1b-ecca-45b7-8434-c2d21c80187b" Variant="1" />
									</Commands>
								</Response>
								<Response Role="RABIN_POGROM" ReferenceLength="7.26">
									<Parenthetical>
										<Text StringName="sam__rabin_trochu_pobavene_plrG" Text="trochu pobaveně" />
									</Parenthetical>
									<Text StringName="sam__rabin_dobra_dobra_ch_K9Ng" Text="Gut gut, jingele. Zorg zich ništ. Es iz ništ vert kejn cibele. ">
										<Note StringName="sam__rabin_dobra_dobra_ch_K9Ng_note" Text="celé v jidiš: Dobrá dobrá, chlapče. Jen jsem nechtěl, aby sis dělal zbytečné starosti." />
									</Text>
									<Commands>
										<AnimationCommand Delay="4.25" FragmentId="ADLG_Emphasis" Guid="ef25df68-e606-4b6a-b97d-e75270c52e55" Variant="7" />
										<AnimationCommand Role="SAMUEL_POGROM" Delay="7.22" FragmentId="ADLG_Confusion" Guid="19e1b926-8753-4893-af45-10d39fa3a985" Variant="0" />
									</Commands>
									<ReviewNotesStylistic>
										<Item Date="2022-01-05 09:39:54" UserName="vladimir.marecek" Text="tady bych asi tu větu &quot;já budu rád, když nás doprovodíš.&quot; vynechal. Moc mi nezapadá k tomu předchozímu a funguje to i bez ní. Nebo pokud jí tam chceš nechat, tak bych řekl &quot;já budu koneckonců rád, když nás doprovodíš.&quot; To by mi líp zapadlo." AcceptState="Accepted" />
									</ReviewNotesStylistic>
								</Response>
								<Response Role="SAMUEL_POGROM" ReferenceLength="2.96">
									<Parenthetical>
										<Text StringName="sam__samuel_zvedne_hlas_p_bBwc" Text="zvedne hlas, popuzeně" />
									</Parenthetical>
									<Text StringName="sam__samuel_co_to_bude_yszy" Text="Ništ vert kejn cibele? Bistu ernst?! ">
										<Note StringName="sam__samuel_co_to_bude_yszy_note" Text="celé v jidiš: Zbytečný starosti? To nemyslíš vážně, ne?" />
									</Text>
									<Commands>
										<AnimationCommand Role="RABIN_POGROM" Delay="2.31" FragmentId="ADLG_Gesture" Guid="5a848a01-c523-4d5c-888d-3198922fecf3" Variant="7" DesiredDuration="6.05" />
									</Commands>
								</Response>
								<Response Role="RABIN_POGROM">
									<Parenthetical>
										<Text StringName="sam__rabin_uklidnuje_ho_xe3m" Text="uklidňuje ho" />
									</Parenthetical>
									<Text StringName="sam__rabin_nikdy_zadnymu__FD3t" Text="Jom iro, ani ejlejcho evtoch. Ven ich hob gehat mojres, bin ich sojmech ojf ha-šem. ">
										<Note StringName="sam__rabin_nikdy_zadnymu__FD3t_note" Text="celé v jidiš: Klidně si můžeš rvát vlasy strachem, ale bude to k ničemu. Člověk nikdy neví, co se bude dít." />
									</Text>
								</Response>
								<Response Role="SAMUEL_POGROM" StartDelay="0.06" ReferenceLength="5.2">
									<Parenthetical>
										<Text StringName="sam__samuel_opet_tise_HC6g" Text="opět tiše" />
									</Parenthetical>
									<Text StringName="sam__samuel_to_davno_vim__4PMY" Text="Dos hostu mir šojn azoj oft gezogt. Ober es iz noch šver far mir.">
										<Note StringName="sam__samuel_to_davno_vim__4PMY_note" Text="celé v jidiš: To mi říkáš často, ale stejně se s tím nedokážu smířit." />
									</Text>
									<Commands>
										<MoodCommand Role="RABIN_POGROM" Delay="3.93" Mood="pensive" />
										<AnimationCommand Delay="-0.04" FragmentId="ADLG_Gesture" Guid="7fcd8a44-bad1-4106-bc29-4498fa7b7196" Variant="13" />
										<AnimationCommand Delay="3.58" FragmentId="ADLG_posture_change" Guid="a2b86c17-a70d-46ad-b9fa-4a06f322264a" Variant="6" />
									</Commands>
								</Response>
								<Response Role="RABIN_POGROM">
									<Text StringName="sam__rabin_poradne_si_umy_Dcnq" Text="Mir kenen šmuesn vegn dos ojfn veg. Ober kojdem gej un ze ojb emecer darf noch hilfn.">
										<Note StringName="sam__rabin_poradne_si_umy_Dcnq_note" Text="celé v jidiš: Můžeme se o tom pobavit po cestě... Ale teď se jdi podívat, jestli někdo ještě něco nepotřebuje." />
									</Text>
								</Response>
								<Response Role="SAMUEL_POGROM" StartDelay="0.19" ReferenceLength="0.93">
									<Parenthetical>
										<Text StringName="sam__samuel_po_povzdechne_a9vx" Text="povzdechne si" />
									</Parenthetical>
									<Text StringName="sam__samuel_chapu_sbohem__H09Z" Text="Nu, gut…">
										<Note StringName="sam__samuel_chapu_sbohem__H09Z_note" Text="celé v jidiš: Tak dobře..." />
									</Text>
									<Commands>
										<AnimationCommand Delay="-0.11" FragmentId="ADLG_Nod" Guid="f219ea89-720f-4384-b0f6-57044649e8dd" Variant="4" />
									</Commands>
								</Response>
