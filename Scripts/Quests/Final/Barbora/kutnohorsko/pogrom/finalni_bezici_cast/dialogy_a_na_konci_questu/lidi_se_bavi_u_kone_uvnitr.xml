	<Skald>
		<Dialog Name="lidi_se_bavi_u_kone_uvnitr">
			<Text StringName="lidi_se_bavi_u_kone_uvnitr_lqvC" Text="Lidi se bavi u koně uvnitř" />
			<Dialogue Type="ingame" TechnicalStatus="Enabled" Initiator="NonPlayer">
				<Decision Name="dec1" Priority="MainQuestSubmission" Alias="pogrom_lidiSeBaviUKoneUvnitr">
					<Sequences>
						<Sequence EndType="EndDialogue" Name="seq1">
							<DesignName StringName="lidi_se_b_seq1_FmAU" Text="seq1" />
							<Elements>
								<Response Role="POGROM_U_KONE_MUZ_1">
									<Text StringName="lidi_pogrom_u__neboj_se_i_3YKH" Text="Neboj se, i kdyby k tomu došlo, tak ten vůz utáhne... Však se na něj podívej." />
								</Response>
								<Response Role="POGROM_U_KONE_MUZ_2">
									<Text StringName="lidi_pogrom_u__asi_ho_sam_jL6N" Text="No jo, utáhne... Ale nebude dost rychlej." />
								</Response>
								<Response Role="POGROM_U_KONE_MUZ_1">
									<Text StringName="lidi_pogrom_u__jak_rikam__FgCS" Text="Neboj se, zatím to nějak vydržíme a za chvíli nám přivede druhýho. " />
								</Response>
								<Response Role="POGROM_U_KONE_MUZ_2">
									<Text StringName="lidi_pogrom_u__ale_co_kdy_ef3R" Text="Ale co když si ho někdo všimne?" />
								</Response>
								<Response Role="POGROM_U_KONE_MUZ_1">
									<Text StringName="lidi_pogrom_u__vsak_je_ne_Lojj" Text="No a? Vždyť ty koně nekrade, ale kupuje! " />
								</Response>
								<Response Role="POGROM_U_KONE_MUZ_1">
									<Text StringName="lidi_pogrom_u__navic_se_z_fTwL" Text="Navíc se zná se spoustou handlířů v okolí... Určitě se najde aspoň jeden, kterej se nad ním slituje a pomůže nám." />
								</Response>
							</Elements>
						</Sequence>
					</Sequences>
				</Decision>
			</Dialogue>
		</Dialog>
	</Skald>
</Database>