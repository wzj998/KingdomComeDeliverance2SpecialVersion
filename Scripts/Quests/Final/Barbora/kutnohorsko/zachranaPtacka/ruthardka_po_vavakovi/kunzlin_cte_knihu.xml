											<Response Role="KUNCLIN_RUTHARD">
												<Parenthetical>
													<Text StringName="kunz_kunclin_r_prekvapene_4bEb" Text="překvapeně" />
												</Parenthetical>
												<Text StringName="kunz_kunclin_r_corrump_IEtG" Text="Corrump?" />
