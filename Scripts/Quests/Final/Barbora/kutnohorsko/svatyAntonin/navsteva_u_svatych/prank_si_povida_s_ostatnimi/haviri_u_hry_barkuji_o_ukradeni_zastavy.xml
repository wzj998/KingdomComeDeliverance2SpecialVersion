									<Parenthetical>
										<Text StringName="havi_svaty_prv_vyziva_EeJM" Text="vyzývá" />
									</Parenthetical>
									<Text StringName="havi_svaty_prv_hej_tuplak_hbYP" Text="Hej, Tupláku, hraješ, nebo chrápeš?" />
								</Response>
								<Response Role="SVATY_DRUHY">
									<Parenthetical>
										<Text StringName="havi_svaty_dru_mrzute_Vozc" Text="mrzutě" />
									</Parenthetical>
									<Text StringName="havi_svaty_dru_hm_jo_uz_t_ddAg" Text="No jo, hned to bude." />
