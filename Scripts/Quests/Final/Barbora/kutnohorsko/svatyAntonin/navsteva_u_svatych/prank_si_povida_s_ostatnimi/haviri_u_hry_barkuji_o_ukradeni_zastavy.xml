								<Response Role="JORG_PRANK">
									<Parenthetical>
										<Text StringName="havi_jorg_prank_s_prevahou_Zcam" Text="s převahou" />
									</Parenthetical>
									<Text StringName="havi_jorg_prank_leda_hovn_xFmI" Text="Leda hovno. A i kdyby, hen jim ten jejich svatej Antonín už nepomůže!" />
								</Response>
								<Response Role="SVATY_PRVNI">
									<Parenthetical>
										<Text StringName="havi_svaty_prv_po_odmlce__M9fQ" Text="po odmlce komentuje kostky" />
									</Parenthetical>
									<Text StringName="havi_svaty_prv_uz_zase_tu_Wldu" Text="No co to, Tuplák? Už zase?" />
