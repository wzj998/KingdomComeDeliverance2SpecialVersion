								<Response Role="SVATY_DRUHY">
									<Parenthetical>
										<Text StringName="havi_svaty_dru_naivne_izUJ" Text="naivně, trochu s respektem" />
									</Parenthetical>
									<Text StringName="havi_svaty_dru_no_treba_j_Rkjl" Text="No, třeba je ta jejich švancara vočarovaná...?" />
