								<Response Role="SVATY_PRVNI">
									<Parenthetical>
										<Text StringName="havi_svaty_prv_sebevedome_uJOt" Text="sebevědomě" />
									</Parenthetical>
									<Text StringName="havi_svaty_prv_streleckej_8Quj" Text="Střeleckej šterc máme v kapse, co?" />
								</Response>
								<Response Role="JORG_PRANK">
									<Parenthetical>
										<Text StringName="havi_jorg_prank_prikyvne__U5F8" Text="přikývne, rozhodí rukama" />
									</Parenthetical>
									<Text StringName="havi_jorg_prank_ha_naturl_4AEm" Text="Ja, natürlich! Beztak to Sešívaný ňák šmelej, že ten šterc dycky vyhrajou." />
