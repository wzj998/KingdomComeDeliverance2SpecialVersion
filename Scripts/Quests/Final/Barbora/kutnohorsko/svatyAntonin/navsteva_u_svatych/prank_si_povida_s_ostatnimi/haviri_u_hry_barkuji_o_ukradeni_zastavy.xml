								<Response Role="SVATY_PRVNI">
									<Parenthetical>
										<Text StringName="havi_svaty_prv_pohorsene__TkHW" Text="pohoršeně vyzývá" />
									</Parenthetical>
									<Text StringName="havi_svaty_prv_no_to_je_d_Fa9a" Text="To je dost. Brals fest geld za tu švancaru, tak to trochu roztoč, ne?!" />
								</Response>
								<Response Role="SVATY_DRUHY">
									<Parenthetical>
										<Text StringName="havi_svaty_dru_hudruje_9zzB" Text="hudruje" />
									</Parenthetical>
									<Text StringName="havi_svaty_dru_hm_po_tom__EWNR" Text="Po tom ti je akorát hovno, Šipajz." />
								</Response>
								<Response Role="SVATY_PRVNI">
									<Parenthetical>
										<Text StringName="havi_svaty_prv_predklada__KPS1" Text="předkládá v dobrým, uchechtne se" />
									</Parenthetical>
									<Text StringName="havi_svaty_prv_ale_no_tak_5ll6" Text="Dyť to sou vlastně i mý prachy. Kdo by řek, že ji starýmu Lazaroj štípneš přímo pod nosem, hehe." />
								</Response>
								<Response Role="SVATY_DRUHY">
									<Parenthetical>
										<Text StringName="havi_svaty_dru_hudruje_PAEF" Text="hudruje" />
									</Parenthetical>
									<Text StringName="havi_svaty_dru_nemel_ses__Z75o" Text="Neměl ses sázet!" />
