							<Elements>
								<Response Role="SAMUEL" ReferenceLength="2.05">
									<Text StringName="samu_samuel_chces_kouk_Ayzx" Text="Tak co, viděl si něco?" />
									<Commands>
										<FacialMoodCommand FacialMood="thinking2" />
										<AnimationCommand Role="SAMUELUV_SPION_1" Delay="2.04" FragmentId="ADLG_Disagree_light" Guid="e640819e-2655-4e4c-94c2-1e04c5dd7090" Variant="4" />
									</Commands>
								</Response>
								<Response Role="SAMUELUV_SPION_1" ReferenceLength="7.07">
									<Text StringName="samu_samueluv__chci_ale_c_HPKV" Text="Ne, myslim že si že chytli vítr. Nikdo se neukázal. Vokna, dveře zavřený. Neviděl sem dovnitř." />
									<Commands>
										<FacialMoodCommand FacialMood="thinking1" />
										<AnimationCommand Delay="2.58" FragmentId="ADLG_Confusion" Guid="5da7fd8b-7cec-40c8-a723-f3cc47022f36" Variant="4" />
										<AnimationCommand Delay="7.06" FragmentId="ADLG_Gesture" Guid="002ace53-8d31-4d61-b729-4bae5c80b2e5" Variant="0" />
									</Commands>
								</Response>
								<Response Role="SAMUELUV_SPION_1" ReferenceLength="5.57">
									<Text StringName="samu_samueluv__coze_fakt_mjoK" Text="A ten gój co vopravuje barák naproti chce víc peněz za to že mě tam nechá vočumovat z lešení." />
									<Commands>
										<AnimationCommand Role="SAMUEL" Delay="5.48" FragmentId="ADLG_Disagree" Guid="9befa935-0e58-4e32-ae2a-a033cc94f2e2" Variant="3" />
									</Commands>
								</Response>
								<Response Role="SAMUEL" StartDelay="0.04">
									<Text StringName="samu_samuel_jo_tak_se_me__DWzg" Text="Nic mu nedáme, stejně se v tomhle domě už nikdo neukáže." />
								</Response>
								<Response Role="SAMUELUV_SPION_1" StartDelay="0.06">
									<Text StringName="samu_samueluv__jsi_polda_AaCh" Text="Co když si bude stěžovat?" />
								</Response>
								<Response Role="SAMUEL" StartDelay="0.06" ReferenceLength="3.78">
									<Text StringName="samu_samuel_ne_ZMFw" Text="To pochybuju a kdyby, tak si ho najdeme." />
									<Commands>
										<FacialMoodCommand FacialMood="thinking1" />
										<AnimationCommand Delay="1.57" FragmentId="ADLG_Gesture" Guid="82614ed5-8978-4c2d-8771-aeab0b05a4e7" Variant="14" />
									</Commands>
								</Response>
								<Response Role="SAMUELUV_SPION_1" StartDelay="0.1" ReferenceLength="1.02">
									<Text StringName="samu_samueluv__za_kolik_d_ZeWe" Text="A co dál?" />
									<Commands>
										<AnimationCommand Delay="-0.09" FragmentId="ADLG_Confusion" Guid="3ee18b27-3ab6-44ff-973a-dac45dd39426" Variant="1" />
									</Commands>
								</Response>
								<Response Role="SAMUEL" StartDelay="0.11" ReferenceLength="4.6">
									<Text StringName="samu_samuel_tri_tisice_za_WP5J" Text="Počkáme s čím přijdou ostatní. Ale někde se určitě scházej. Tím sem si jistej." />
									<Commands>
										<AnimationCommand Delay="-0.02" FragmentId="ADLG_Emphasis" Guid="ef25df68-e606-4b6a-b97d-e75270c52e55" Variant="7" />
									</Commands>
								</Response>
								<Response Role="SAMUELUV_SPION_1" StartDelay="0.08" ReferenceLength="3.78">
									<Text StringName="samu_samueluv__tvle_tak_j_K9fA" Text="Za město nejezděj. Co když si vyměňujou psaní?" />
									<Commands>
										<AnimationCommand Delay="0.01" FragmentId="ADLG_Gesture" Guid="772acf5a-6278-4ba5-b562-50981cf99cda" Variant="12" />
										<AnimationCommand Role="SAMUEL" Delay="3.48" FragmentId="ADLG_Think" Guid="72b8c58f-a8d1-44e5-bf27-0558d5e03228" Variant="1" DesiredDuration="4.52" />
									</Commands>
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_policie_jmene_ohpe" Text="Hm ... to mě nenapadlo. Jenže všechno služebnictvo sledovat nemůžeme. Je jich jak Filištínů." />
								</Response>
								<Response Role="SAMUELUV_SPION_1" ReferenceLength="5.8">
									<Text StringName="samu_samueluv__ale_ty_si__9uRt" Text="Mohli bysme nějaký pochytat a trochu se vyptat. Zjistim který z nich neuměj číst." />
									<Commands>
										<AnimationCommand Delay="3.35" FragmentId="ADLG_Gesture" Guid="f15da38f-1fd7-45d6-9587-0b74338f13fc" Variant="24" />
										<AnimationCommand Role="SAMUEL" Delay="5.75" FragmentId="ADLG_Nod" Guid="f219ea89-720f-4384-b0f6-57044649e8dd" Variant="4" />
									</Commands>
								</Response>
								<Response Role="SAMUEL" StartDelay="0.08" ReferenceLength="7.32">
									<Text StringName="samu_samuel_ty_si_ale_deb_jGXM" Text="Dobrej nápad. Ale udělej to nějak nenápadně. Třeba někdo z nich někomu dluží. Navlíkneme to na to." />
									<Commands>
										<AnimationCommand Delay="1.25" FragmentId="ADLG_Whisper" Guid="ab7e9df3-ca07-5c59-a645-70a47952b384" Variant="1" DesiredDuration="5.49" />
									</Commands>
								</Response>
								<Response Role="SAMUELUV_SPION_1" StartDelay="0.11" ReferenceLength="1.46">
