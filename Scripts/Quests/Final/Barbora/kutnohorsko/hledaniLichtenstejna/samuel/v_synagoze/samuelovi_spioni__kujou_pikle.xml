<?xml version="1.0" encoding="utf-8"?>
<Database xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="brambora">
	<Skald>
		<Dialog Name="samuelovi_spioni__kujou_pikle">
			<Text StringName="samuel_spioni__kuti_pikli_4Hy9" Text="Samuelovi špióni - Kujou pikle" />
			<Elements>
				<GameplayActionElement>
					<Text StringName="samuel_sp_samuelovi_lide__sTWk" Text="Samuelovi lidé si před synagogou povídají šeptem." />
				</GameplayActionElement>
			</Elements>
			<Dialogue Type="ingame" TechnicalStatus="Enabled" DefaultMaxDistance="10" Initiator="NonPlayer">
				<Decision Name="dec1" Priority="MainQuestSubmission" Alias="hledaniLichtenstejna_samuelsSpies_scheming">
					<Sequences>
						<Sequence EndType="EndDialogue" Name="seq1">
							<DesignName StringName="samuel_sp_seq1_Cs3B" Text="seq1" />
							<Elements>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_pikle_kujme_f6vk" Text="Kolik že jich tam bylo?" />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuelovi_kujme_pikle_QMvn" Text="Jen dva. Ani sluhové, ani doprovod." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_pikle_pikle___JF9V" Text="A který to byli?" />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuelovi_kujme_kujme_ZmZg" Text="Moc se v nich nevyznám. Byl tam ten vysokej s knírkem." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_sp_hm__talhof_t1f2" Text="Hm... Talhofer, to je pěknej zmetek." />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_jo__ten_ta_3OLa" Text="Jo, ten tam byl. A bavili se s nějakym maďarem - Gábor se snad jmenoval." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_sp_a_o_cem_c84R" Text="O čem?" />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_prevazne_o_n52d" Text="Převážně o víně." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_sp_a_to_je_vs_Mj4v" Text="A to je všechno?" />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_jo__byli_t_HR8W" Text="Jo. Byli tam celý dopoledne, tlachali, chlastali a jedli." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_sp_a_nebyl_to_0wnz" Text="A nebyla to nějaká šifra?" />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_ja_nevim_n_2sD1" Text="Nepřišlo mi... Navíc se pak stočila řeč na ženský." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_sp_a_to_nerik_eWyF" Text="Tam taky nic zajímaýho nepadlo? " />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_jakoze_by__b6AV" Text="Leda že by &quot;prdelatá služka Julie, kterou Tlahofer každej den po ránu pígluje po snídani&quot; byla krycí jméno pro někoho důležitýho." />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samueluv__ale_dost_p_kXf0" Text="Ale dost pochybuju." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_sp_mas_pravdu_ccp2" Text="Máš pravdu, tak chytrý nejsou." />
								</Response>
								<Response Role="SAMUEL">
									<Parenthetical>
										<Text StringName="samu_samueluv__pobavene_Syk7" Text="pobaveně" />
									</Parenthetical>
									<Text StringName="samu_samuel_sp_ha_no_to_j_lHER" Text="O těch novejch radních se nebavili?" />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_ne_ani_slu_Mwb5" Text="Ani slůvko. Leda by to stihli za tu chvilku, co sem je neslyšel. " />
								</Response>
								<Response Role="SAMUEL">
									<Parenthetical>
										<Text StringName="samu_samueluv__zbystri_0N9m" Text="zbystří" />
									</Parenthetical>
									<Text StringName="samu_samuel_sp_jakto_0nHo" Text="Jak to?" />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_no_vetsinu_zt97" Text="Většinu času sem je obsluhoval. Dolejval jim poháry a tak. Jenže pak si mě poslali pro něco do kuchyně." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_sp_a_kde_byl__qXLa" Text="Copak tam nebyl Izaš? Měl čekat za dveřma a všechno ti donýst!" />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_ten_pitome_zOfL" Text="Ten pitomec se někam ztratil! Nejspíš někde balil děvečky, nýmand jeden. Takže jsem musel běžet do kuchyně a zpátky." />
								</Response>
								<Response Role="SAMUELUV_SPION_2">
									<Text StringName="samu_samuel_sp_cestou_sem_nulm" Text="Cestou sem málem vysypal kachní paštiku na schody." />
								</Response>
								<Response Role="SAMUEL">
									<Text StringName="samu_samuel_sp_hm__mohli__YEY1" Text="Hm...  To je bezvadný... " />
								</Response>
							</Elements>
						</Sequence>
					</Sequences>
				</Decision>
			</Dialogue>
		</Dialog>
	</Skald>
