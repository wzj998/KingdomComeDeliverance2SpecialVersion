											<Text StringName="cin__samuel_pockej_nezabi_vDlP" Text="Počkej! Nezabíjej je! " />
										</Response>
									</Elements>
								</Sequence>
							</Sequences>
						</Decision>
					</Dialogue>
				</LinearDialogueElement>
				<SceneActionElement>
					<Text StringName="cin_m3330_jindrich_vidi_z_xEg7" Text="Jindřich vidí, že je neozbrojen, tak se trochu zrelaxuje." />
