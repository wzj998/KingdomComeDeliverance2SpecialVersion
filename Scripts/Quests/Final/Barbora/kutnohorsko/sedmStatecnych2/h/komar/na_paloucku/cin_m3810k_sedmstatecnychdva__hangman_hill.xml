											<Parenthetical>
												<Text StringName="cin__henry_vzrusene_6BFK" Text="vzrušeně" />
											</Parenthetical>
											<Text StringName="cin__henry_pockej_zastav_VTAX" Text="Počkej! Zastav!" />
