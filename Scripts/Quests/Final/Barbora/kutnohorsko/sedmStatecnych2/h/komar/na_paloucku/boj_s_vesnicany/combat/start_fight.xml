				</Port>
				<Port Name="hrac_vyhral" Direction="Out" Type="trigger">
					<DesignName StringName="start_fig_hrac_vyhral_WItE" Text="Hrac vyhral" />
				</Port>
				<Port Name="hrac_utekl_z_bitky" Direction="Out" Type="trigger">
