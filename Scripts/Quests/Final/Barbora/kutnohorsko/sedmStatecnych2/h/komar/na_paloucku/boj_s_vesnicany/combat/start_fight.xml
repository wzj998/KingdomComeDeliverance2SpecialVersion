				</Output>
				<skirmish Name="skirmish" PositionY="40" PositionX="90" Namespace="utils.combat">
					<Asset Name="souls1" Alias="villagersAndButcher" />
					<Asset Name="souls2" Alias="player" />
					<Edge From="start_boj_s_vesnicany" To="isactive" />
				</skirmish>
