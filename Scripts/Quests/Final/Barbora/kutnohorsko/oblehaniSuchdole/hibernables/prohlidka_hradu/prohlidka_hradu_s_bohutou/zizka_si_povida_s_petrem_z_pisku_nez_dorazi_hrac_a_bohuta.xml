									</Parenthetical>
									<Text StringName="zizk_petr_pisek_a_co_s_ni_KuR1" Text="A k čemu nám bude stříbro, když nás tady stejně vyhladoví?" />
									<Commands>
										<FacialMoodCommand FacialMood="sad1" />
										<AnimationCommand Delay="0.46" FragmentId="ADLG_Confusion" Guid="5da7fd8b-7cec-40c8-a723-f3cc47022f36" Variant="4" />
