								<Response Role="JAN_ZIZKA_Z_TROCNOVA" ReferenceLength="4.21">
									<Parenthetical>
										<Text StringName="zizk_zizka_s_klidem_xiIL" Text="s klidem" />
									</Parenthetical>
									<Text StringName="zizk_zizka_a_co_bychom_me_nmhv" Text="A co bychom měli udělat? To stříbro jim prostě s omluvou věnovat?" />
									<Commands>
