			<Elements>
				<GameplayActionElement>
					<Text StringName="modlitba__jindra_se_po_po_IfWV" Text="Jindra se po pokřižování pomodlí za Samuela." />
				</GameplayActionElement>
			</Elements>
			<Dialogue Type="ingame monolog" TechnicalStatus="Enabled" Initiator="NonPlayer" ClashPriority="Technical" GesturesNotNeeded="true">
				<Decision Name="dec1" Priority="MainQuestSubmission" Alias="finale_modlitbaZaSama">
					<Sequences>
						<Sequence EndType="EndDialogue" Name="seq2">
							<DesignName StringName="modlitba__seq2_2TfE" Text="seq2" />
							<Elements>
								<Response Role="HENRY">
									<Parenthetical>
										<Text StringName="modl_henry_nejiste_mGYC" Text="modlí se" />
									</Parenthetical>
									<Text StringName="modl_henry_placeholder_mo_54OA" Text="Bože všemohoucí, Otče náš na nebesích. " />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_dej_klid_dusi__J9xV" Text="Dej klid duši mého zesnulého bratra Samuela. Byl jiný víry, než já, ale zaslouží si tvoje milosrdenství." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_nemel_jsem_cas_bWeB" Text="Neměl jsem čas ho poznat tak dobře jak bych si přál. Ale byl to dobrej člověk a nezasloužil si zemřít." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_i_kdyz_jsi_ho__NFqz" Text="I když jsi ho k sobě povolal, dej aby netrpěl dlouho v očistci a aby jeho památka nikdy nezhynula." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_prisaham_pred__2M1m" Text="Přísahám před tebou a všemi svatými, že na něj nikdy nezapomenu." />
								</Response>
								<Response Role="HENRY">
									<Text StringName="modl_henry_amen_eMSV" Text="Amen." />
								</Response>
							</Elements>
						</Sequence>
					</Sequences>
