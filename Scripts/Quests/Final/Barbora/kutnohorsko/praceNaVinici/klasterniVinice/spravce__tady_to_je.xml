											<Response Role="SPRAVCE_VINICE_JERONYM">
												<Text StringName="spra_spravce_v_mlceti_zla_2Dlk" Text="Mlčeti zlato... Sám o tom něco vím." />
											</Response>
										</Elements>
									</Sequence>
								</Sequences>
							</Decision>
						</Sequence>
