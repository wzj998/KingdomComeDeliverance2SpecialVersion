												<Text StringName="spra_henry_bezva_Pixo" Text="Bezva." />
											</Response>
										</Elements>
									</Sequence>
									<Sequence EndType="GoTo" GoToDecision="dec3" ChatPosition="Refusal" Name="seq7">
