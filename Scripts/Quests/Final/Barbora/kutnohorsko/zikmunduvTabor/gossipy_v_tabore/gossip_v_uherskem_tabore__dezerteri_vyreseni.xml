								<Response Role="UHERSKY_VOJAK_2">
									<Parenthetical>
										<Text StringName="goss_uhersky_v_otravene_fdhf" Text="otráveně" />
									</Parenthetical>
									<Text StringName="goss_uhersky_v_ale_co_je__miUV" Text="A co je mi po tom? Prostě se zvednu a půjdu, ne? Když to mohli udělat voni..." />
