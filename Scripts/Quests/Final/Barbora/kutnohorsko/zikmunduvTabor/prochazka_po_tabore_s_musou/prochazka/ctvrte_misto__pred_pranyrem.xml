		<Dialog Name="ctvrte_misto__pred_pranyrem">
			<Text StringName="ctvrte_misto__pred_pranyr_fqtG" Text="čtvrté místo - před pranýřem" />
			<Dialogue Type="ingame monolog" TechnicalStatus="Enabled" Initiator="NonPlayer">
				<Decision Name="dec1" Priority="MainQuestAdditionals" Alias="zikmunduvTabor_ctvrteMistoPredPranyrem">
					<Sequences>
						<Sequence EndType="Decision" Name="seq2">
							<DesignName StringName="ctvrte_mi_seq2_6eCV" Text="seq2" />
							<Elements>
								<Response Role="MUSA_Z_MALI">
									<Parenthetical>
										<Text StringName="ctvr_musa_z_ma_jako_pruvo_Jgqy" Text="jako průvodce, přátelsky" />
									</Parenthetical>
									<Text StringName="ctvr_musa_z_ma_tady_je_zd_2TlF" Text="No a tady nahoře se usadil Katzův oddíl pražských dělostřelců. A to k velké nelibosti všech ostatních v táboře, musím říct." />
								</Response>
								<Response Role="MUSA_Z_MALI">
									<Parenthetical>
										<Text StringName="ctvr_musa_z_ma_zasmeje_se_BW92" Text="zasměje se" />
									</Parenthetical>
									<Text StringName="ctvr_musa_z_ma_to_vis_nej_SYKc" Text="To víš, nejlepší místo, hned vedle hejtmanů. A když zaprší, všechna špína odtud teče z kopce dolů ke kumánům a žoldákům." />
								</Response>
								<Response Role="MUSA_Z_MALI">
									<Text StringName="ctvr_musa_z_ma_tady_je_pr_DR4f" Text="Tady je pranýř. Kéž je Alláh milostiv a nikdy na něm neskončíš. A pokud obejdeš kopec, narazíš na zdejší střelnici." />
								</Response>
								<Response Role="MUSA_Z_MALI">
									<Parenthetical>
										<Text StringName="ctvr_musa_z_ma_jako_pruvo_HwvY" Text="jako průvodce, přátelsky" />
									</Parenthetical>
									<Text StringName="ctvr_musa_z_ma_a_pokud_te_9ar7" Text="Vřele doporučuji se tam zastavit. Kumáni se snad rodí s lukem v ruce! Ale až tam tě nedoprovodím, příteli." />
								</Response>
								<Response Role="MUSA_Z_MALI">
									<Parenthetical>
										<Text StringName="ctvr_musa_z_ma_pratelsky__MwqS" Text="přátelsky, pokloní se" />
									</Parenthetical>
									<Text StringName="ctvr_musa_z_ma_a_tady_te__aBxj" Text="Nerad nechávám špitál dlouho bez dozoru." />
								</Response>
							</Elements>
							<Decision Name="dec2" Priority="MainQuestSubmission">
								<Sequences>
									<Sequence EndType="EndDialogue" Name="cela_prochazka_dokoncena">
										<DesignName StringName="ctvrte_mi_seq1_NI7s" Text="celá procházka dokončena" />
										<Elements>
											<Response Role="MUSA_Z_MALI">
												<Parenthetical>
													<Text StringName="ctvr_musa_z_ma_louci_se_b_Y2uR" Text="loučí se, bambarsky" />
												</Parenthetical>
												<Text StringName="ctvr_musa_z_ma__ala_ka_an_h1Gc" Text=" Ala ka an ben!" Fixed="true">
													<Note StringName="ctvr_musa_z_ma__ala_ka_an_h1Gc_note" Text="goodbye in bambara (malian language)" />
												</Text>
											</Response>
										</Elements>
									</Sequence>
								</Sequences>
							</Decision>
						</Sequence>
					</Sequences>
