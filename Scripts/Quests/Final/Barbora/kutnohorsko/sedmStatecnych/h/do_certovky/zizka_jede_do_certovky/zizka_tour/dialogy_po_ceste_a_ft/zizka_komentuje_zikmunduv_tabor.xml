									<Sequence SpeechCoef="0" EndType="Decision" Name="seq2">
										<DesignName StringName="zizka_kom_seq2_cdpG" Text="seq2" />
										<UiPrompt StringName="ui_zizka_kom_seq2_civV" Text="Čekal jsem problémy" />
