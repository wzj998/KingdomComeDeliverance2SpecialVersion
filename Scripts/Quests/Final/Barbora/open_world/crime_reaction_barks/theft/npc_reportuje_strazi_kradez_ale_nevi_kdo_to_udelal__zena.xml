											<Response Role="NPC_REPORTUJE_STRAZI_KRADEZ_NEVI_O_HRACI_ZENA">
												<Text StringName="npc__npc_repor_vubec_nevi_woGK" Text="Vůbec nevím, kdo to mohl udělat!" />
