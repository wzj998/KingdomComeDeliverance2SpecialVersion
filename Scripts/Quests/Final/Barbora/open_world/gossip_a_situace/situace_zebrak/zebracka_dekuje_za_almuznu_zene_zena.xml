										<Elements>
											<Response Role="ZEBRAK_DEKUJE_ZA_ALMUZNU_ZENA">
												<Parenthetical>
													<Text StringName="zebr_zebrak_de_uctive_vde_vx9i" Text="uctivě, vděčně" />
												</Parenthetical>
												<Text StringName="zebr_zebrak_de_diky_dekuj_5rez" Text="Díky... Děkuju ženo! Bůh vidí tvojí štědrost a odmění se ti." />
											</Response>
										</Elements>
									</Sequence>
									<Sequence EndType="EndDialogue" Cooldown="5m" Name="seq3_1">
										<DesignName StringName="zebracka__seq3_1_wWiX" Text="seq3" />
