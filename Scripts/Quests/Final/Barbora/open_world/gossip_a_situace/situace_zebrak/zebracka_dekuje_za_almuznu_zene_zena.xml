												</Parenthetical>
												<Text StringName="zebr_zebrak_de_diky_dobra_GQN3" Text="Díky, dobrá duše. Bůh ti to jistě vynahradí nastokrát." />
											</Response>
										</Elements>
									</Sequence>
									<Sequence EndType="EndDialogue" Cooldown="5m" Name="seq5_1">
										<DesignName StringName="zebracka__seq5_1_QGO9" Text="seq5" />
