												<Parenthetical>
													<Text StringName="zebr_zebrak_de_vdecne_una_7duV" Text="vděčně, unaveně" />
												</Parenthetical>
												<Text StringName="zebr_zebrak_de_diky_pani__cnI8" Text="Díky paní, při Kristu na kříži, děkuju." />
											</Response>
										</Elements>
									</Sequence>
									<Sequence EndType="EndDialogue" Cooldown="5m" Name="seq7_1">
