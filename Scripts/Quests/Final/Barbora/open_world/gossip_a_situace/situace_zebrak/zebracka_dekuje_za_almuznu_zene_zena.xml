												<Parenthetical>
													<Text StringName="zebr_zebrak_de_vdecne_WMKb" Text="vděčně" />
												</Parenthetical>
												<Text StringName="zebr_zebrak_de_diky_dobra_8Nlj" Text="Díky, dobrá ženo. Mockrát děkuju! Ať tě Panna Maria ochraňuje." />
											</Response>
										</Elements>
									</Sequence>
									<Sequence EndType="EndDialogue" Cooldown="5m" Name="seq8_1">
										<DesignName StringName="zebracka__seq8_1_stuV" Text="seq8" />
