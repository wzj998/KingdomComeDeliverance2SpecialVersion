												<Parenthetical>
													<Text StringName="zebr_zebrak_de_uctive_vde_J5Yx" Text="uctivě, vděčně, pokašlává" />
												</Parenthetical>
												<Text StringName="zebr_zebrak_de_dekuju_pan_PTM4" Text="Děkuju. Pánbůh ti to oplatí na dětech, dobrá ženo!" />
											</Response>
										</Elements>
									</Sequence>
									<Sequence EndType="EndDialogue" Cooldown="5m" Name="seq4_1">
