													<Text StringName="zebr_zebrak_de_vdecne_uct_X9nG" Text="vděčně, uctivě" />
												</Parenthetical>
												<Text StringName="zebr_zebrak_de_mockrat_de_yT48" Text="Mockrát děkuju. Máš dobré srdce. Ráda se za tebe pomodlím." />
											</Response>
										</Elements>
									</Sequence>
									<Sequence EndType="EndDialogue" Cooldown="5m" Name="seq10_1">
