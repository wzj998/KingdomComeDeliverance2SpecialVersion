												<Parenthetical>
													<Text StringName="zebr_zebrak_de_uctive_vde_u2w5" Text="uctivě, vděčně" />
												</Parenthetical>
												<Text StringName="zebr_zebrak_de_diky_jedno_Hlz4" Text="Díky. Jednou ti to vrátím. Bůh je mně svědkem, že to udělám!" />
											</Response>
										</Elements>
									</Sequence>
									<Sequence EndType="EndDialogue" Cooldown="5m" Name="seq6_1">
