									<Sequence EntryCondition="Port('jindra_vyhral_zapasy_v_podsemine')" EndType="GoTo" GoToDecision="dec3" Name="seq4">
										<UiPrompt Text="Jsem Krejzlův zápasník.">
											<Localization Text="Jsem Krejzlův zápasník." CreatedOn="2021-11-15T14:31:16.514058" UpdatedOn="2021-11-15T15:04:09.760923" Language="WHS" Translator="martina.sourkova" />
											<Localization Text="I'm Kreyzl's fighter." CreatedOn="2023-10-21T13:57:56.867453" UpdatedOn="2023-10-21T14:32:04.280159" Language="PDG" Translator="tomas.vodslon" />
											<Localization Text="Jsem Krejzlův zápasník." CreatedOn="2024-04-05T18:23:28.860169" UpdatedOn="2024-04-05T18:24:49.715454" Language="CZE" Translator="tomas.vodslon" />
											<Localization Text="I'm Kreyzl's fighter." CreatedOn="2024-04-15T18:23:00.035498" UpdatedOn="2024-04-15T18:37:33.330902" Language="ENG" Translator="tomas.vodslon" />
											<Localization Text="Sou o lutador de Kreyzl." CreatedOn="2024-04-17T13:40:49.299006" UpdatedOn="2024-04-25T18:39:43.142046" Language="POR" Translator="tomas.vodslon" />
											<Localization Text="Я боец от Крейзеля." CreatedOn="2024-05-06T12:28:50.820855" UpdatedOn="2024-05-23T11:28:12.437421" Language="RUS" Translator="tomas.vodslon" />
											<Localization Text="Jestem pięściarzem Krejzla." CreatedOn="2024-05-06T12:32:13.702606" UpdatedOn="2024-05-23T14:58:30.678057" Language="POL" Translator="tomas.vodslon" />
											<Localization Text="Я боєць Крейцля." CreatedOn="2024-05-13T14:40:14.914141" UpdatedOn="2024-05-27T19:20:19.949176" Language="UKR" Translator="tomas.vodslon" />
											<Localization Text="Ich trete für Kreyzl an." CreatedOn="2024-05-17T09:54:46.43917" UpdatedOn="2024-05-29T10:53:24.852551" Language="GER" Translator="tomas.vodslon" />
											<Localization Text="Ben Kreyzl'in dövüşçüsüyüm." CreatedOn="2024-05-17T10:23:39.267972" UpdatedOn="2024-05-29T11:10:43.057746" Language="TUR" Translator="tomas.vodslon" />
										</UiPrompt>
										<Elements>
											<Response Role="HENRY" ReferenceLength="4.34">
												<Text Text="Proto jsem tady, posílá mě mlynář Krejzl z Podsemína.">
													<Localization Text="Proto jsem tady, posílá mě mlynář Krejzl z Podsemína." CreatedOn="2021-11-15T14:31:16.514058" UpdatedOn="2021-11-15T14:49:57.040836" Language="WHS" Translator="martina.sourkova" />
													<Localization Text="That's why I'm here, the miller Kreyzl from Lower Semine sent me." CreatedOn="2023-10-21T13:57:56.867453" UpdatedOn="2023-10-21T14:32:04.280159" Language="PDG" Translator="tomas.vodslon" />
													<Localization Text="That's why I'm here, the miller Kreyzl from Lower Semine sent me." CreatedOn="2024-01-04T15:08:13.707561" UpdatedOn="2024-01-04T16:32:34.85423" Language="ENG" Translator="john.comer" />
													<Localization Text="Вот потому я здесь. Меня мельник Крейзель с Нижнего Семина послал." CreatedOn="2024-02-06T10:13:33.733061" UpdatedOn="2024-02-15T12:26:27.976193" Language="RUS" Translator="tomas.vodslon" />
													<Localization Text="Dlatego tu jestem. Przysłał mnie młynarz Krejzl z Dolnego Semina." CreatedOn="2024-02-06T10:12:22.814537" UpdatedOn="2024-02-15T12:56:29.711457" Language="POL" Translator="tomas.vodslon" />
													<Localization Text="Sono qui per questo. Mi manda il mugnaio Kreyzl di Semine Bassa." CreatedOn="2024-02-28T14:11:15.190364" UpdatedOn="2024-02-28T14:12:21.115528" Language="ITA" Translator="tomas.vodslon" />
													<Localization Text="C'est pour ça que je suis là, c'est Kreyzl, le meunier de Bas-Semine qui m'envoie." CreatedOn="2024-02-28T14:39:35.38927" UpdatedOn="2024-02-28T14:40:26.430237" Language="FRA" Translator="tomas.vodslon" />
													<Localization Text="Por eso estoy aquí. Vengo de parte de Kreyzl, el molinero de Semín Bajo." CreatedOn="2024-02-29T09:54:36.297496" UpdatedOn="2024-02-29T09:57:30.495287" Language="SPA" Translator="tomas.vodslon" />
													<Localization Text="É por isso que estou aqui, o moleiro Kreyzl do Baixo Semine me mandou." CreatedOn="2024-03-14T17:47:03.73569" UpdatedOn="2024-03-14T17:49:19.597281" Language="POR" Translator="tomas.vodslon" />
													<Localization Text="Bu yüzden buradayım, beni Aşağı Semine’den değirmenci Kreyzl gönderdi." CreatedOn="2024-03-12T12:11:03.712503" UpdatedOn="2024-03-21T14:15:48.978208" Language="TUR" Translator="tomas.vodslon" />
													<Localization Text="Deshalb bin ich hier. Müller Kreyzl aus Niedersemin schickt mich." CreatedOn="2024-03-12T12:05:57.296693" UpdatedOn="2024-03-21T14:46:25.069154" Language="GER" Translator="tomas.vodslon" />
													<Localization Text="Proto jsem tady, posílá mě mlynář Krejzl z Podsemína." CreatedOn="2024-04-05T18:23:28.860169" UpdatedOn="2024-04-05T18:24:49.715454" Language="CZE" Translator="tomas.vodslon" />
													<Localization Text="所以我才会在这里，是下塞米的磨坊主克雷泽让我过来的。" CreatedOn="2024-03-26T13:36:54.276837" UpdatedOn="2024-04-12T09:54:47.572675" Language="CHI" Translator="tomas.vodslon" />
													<Localization Text="我正是為此而來，瑟米涅下城的磨坊主人克雷佐介紹我來這裡。" CreatedOn="2024-03-26T13:39:22.110286" UpdatedOn="2024-04-18T10:25:20.759215" Language="ZHO" Translator="tomas.vodslon" />
													<Localization Text="그래서 온 겁니다. 세민 아랫거리의 방앗간지기 크레이즐이 절 보냈죠." CreatedOn="2024-03-26T13:40:49.267668" UpdatedOn="2024-05-20T10:50:34.286371" Language="KOR" Translator="tomas.vodslon" />
													<Localization Text="Тому я тут, мене прислав млинар Крейцль з Нижнього Семина." CreatedOn="2024-05-13T14:40:14.914141" UpdatedOn="2024-05-27T19:20:19.949176" Language="UKR" Translator="tomas.vodslon" />
													<Localization Text="だからここに来たんだ 下セミンの粉屋クレイズルに言われて来た" CreatedOn="2024-03-26T13:42:52.780378" UpdatedOn="2024-05-31T12:59:10.112563" Language="JPN" Translator="tomas.vodslon" />
												</Text>
												<Commands>
													<AnimationCommand Delay="1.65" FragmentId="ADLG_Gesture" Guid="002ace53-8d31-4d61-b729-4bae5c80b2e5" Variant="0" DesiredDuration="1.91" />
													<CameraCommand CameraType="CloseUp" />
												</Commands>
											</Response>
											<Response Role="HOSPODSKY_VAVRINEC_ZELEJOV" ReferenceLength="6.98">
												<Parenthetical>
													<Text Text="potěšeně">
														<Localization Text="potěšeně" CreatedOn="2021-11-15T14:36:13.800714" UpdatedOn="2021-11-15T14:49:57.204982" Language="WHS" Translator="martina.sourkova" />
														<Localization Text="delightedly" CreatedOn="2023-10-21T13:57:56.867453" UpdatedOn="2023-10-21T14:32:04.280159" Language="PDG" Translator="tomas.vodslon" />
														<Localization Text="delightedly" CreatedOn="2023-11-21T10:35:14.626888" UpdatedOn="2023-11-21T11:00:54.713625" Language="ENG" Translator="martin.sourek" />
														<Localization Text="potěšeně" CreatedOn="2024-04-05T18:23:28.860169" UpdatedOn="2024-04-05T18:24:49.715454" Language="CZE" Translator="tomas.vodslon" />
													</Text>
												</Parenthetical>
												<Text Text="Výborně! Kasal se, že tentokrát pošle opravdovýho machra, tak jsem na tebe zvědavej!">
													<Localization Text="Výborně! Kasal se, že tentokrát pošle opravdovýho machra, tak jsem na tebe zvědavej!" CreatedOn="2022-11-14T09:06:28.255583" UpdatedOn="2022-11-14T09:45:08.682437" Language="WHS" Translator="vladimir.marecek" />
													<Localization Text="Wonderful! He boasted he'd be sending a real tough fellah this time, so I'm interested in seeing what you've got!" CreatedOn="2023-10-21T13:57:56.867453" UpdatedOn="2023-10-21T14:32:04.280159" Language="PDG" Translator="tomas.vodslon" />
													<Localization Text="Wonderful! He boasted he'd be sending a real tough fellah this time, so I'm interested in seeing what you've got!" CreatedOn="2023-12-20T09:39:53.380425" UpdatedOn="2023-12-20T12:44:01.529158" Language="ENG" Translator="tomas.vodslon" />
													<Localization Text="Красота! Он похвалялся, что в этом разе настоящего силача пришлет. Вот мне и любопытно на тебя поглядеть." CreatedOn="2023-12-27T12:57:38.337942" UpdatedOn="2024-01-11T08:55:47.707439" Language="RUS" Translator="tomas.vodslon" />
													<Localization Text="太好了！他吹嘘说这次他会派出一个真正厉害的人，所以我真想看看你有什么本事！" CreatedOn="2023-12-26T17:21:26.156353" UpdatedOn="2024-01-11T10:50:12.805508" Language="CHI" Translator="tomas.vodslon" />
													<Localization Text="Wspaniale! Chwalił się, że tym razem przyśle kogoś naprawdę mocnego, więc chętnie zobaczę, co potrafisz!" CreatedOn="2023-12-27T13:01:17.065863" UpdatedOn="2024-01-11T15:03:16.686757" Language="POL" Translator="tomas.vodslon" />
													<Localization Text="Harika! Böbürlenerek bu kez gerçekten sert bir adam göndereceğini söylüyordu, o yüzden yeteneklerini görmeyi çok isterim!" CreatedOn="2023-12-21T15:38:02.969674" UpdatedOn="2024-01-12T11:59:20.612966" Language="TUR" Translator="tomas.vodslon" />
													<Localization Text="Wunderbar! Er prahlte damit, beim nächsten Mal einen wirklich zähen Kerl zu schicken, deshalb bin ich gespannt, was du drauf hast!" CreatedOn="2023-12-21T12:09:19.784151" UpdatedOn="2024-01-12T12:27:28.809201" Language="GER" Translator="tomas.vodslon" />
													<Localization Text="太好了！他誇下海口說這次會派個兇悍的傢伙來，所以我也很想看看你多有本事！" CreatedOn="2023-12-26T17:20:06.459555" UpdatedOn="2024-01-17T09:37:12.205682" Language="ZHO" Translator="tomas.vodslon" />
													<Localization Text="훌륭해! 이번엔 정말 거친 녀석을 보내겠다고 자랑하던데, 실력이 어느 정도인지 궁금하군!" CreatedOn="2023-12-26T17:22:13.240395" UpdatedOn="2024-02-20T12:36:54.852912" Language="KOR" Translator="tomas.vodslon" />
													<Localization Text="素晴らしい！ あいつは本当に強い奴を送り込むと豪語してたからな あんたの実力を見るのが楽しみだ！" CreatedOn="2024-01-04T04:47:15.344706" UpdatedOn="2024-02-22T11:25:31.093961" Language="JPN" Translator="tomas.vodslon" />
													<Localization Text="Splendido! Diceva che avrebbe mandato qualcuno di tosto. Voglio proprio vederti all'opera!" CreatedOn="2024-02-28T14:11:15.190364" UpdatedOn="2024-02-28T14:12:21.115528" Language="ITA" Translator="tomas.vodslon" />
													<Localization Text="Magnifique ! Il s'est vanté d'envoyer un vrai costaud, cette fois, alors j'aimerais bien savoir ce que tu vaux !" CreatedOn="2024-02-28T14:39:35.38927" UpdatedOn="2024-02-28T14:40:26.430237" Language="FRA" Translator="tomas.vodslon" />
													<Localization Text="¡Maravilloso! No dejaba de alardear de que esta vez iba a mandar a alguien que sabía repartir. ¡A ver de qué sois capaz!" CreatedOn="2024-02-29T09:54:36.297496" UpdatedOn="2024-02-29T09:57:30.495287" Language="SPA" Translator="tomas.vodslon" />
													<Localization Text="Ótimo! Ele se gabou de ter mandado um sujeito muito forte desta vez, então quero ver o que você tem a mostrar!" CreatedOn="2024-03-14T17:47:03.73569" UpdatedOn="2024-03-14T17:49:19.597281" Language="POR" Translator="tomas.vodslon" />
													<Localization Text="Výborně! Kasal se, že tentokrát pošle opravdovýho machra, tak jsem na tebe zvědavej!" CreatedOn="2024-04-05T18:23:28.860169" UpdatedOn="2024-04-05T18:24:49.715454" Language="CZE" Translator="tomas.vodslon" />
													<Localization Text="Чудово! Він обіцяв цього разу прислати справжнього крем'язня, тож цікаво подивитися на тебе!" CreatedOn="2024-05-13T14:40:14.914141" UpdatedOn="2024-05-27T19:20:19.949176" Language="UKR" Translator="tomas.vodslon" />
												</Text>
												<Commands>
													<AnimationCommand Delay="0.05" FragmentId="ADLG_Nod" Guid="ca364fe9-66bd-48fa-8178-0e49459d7038" Variant="2" DesiredDuration="1.25" />
													<AnimationCommand Delay="2.56" FragmentId="ADLG_Gesture" Guid="34ac6648-6fd5-4005-84ec-632a56223892" Variant="5" DesiredDuration="1.68" />
