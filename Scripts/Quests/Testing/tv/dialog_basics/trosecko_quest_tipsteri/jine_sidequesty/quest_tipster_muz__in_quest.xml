													<AnimationCommand Delay="0.29" FragmentId="ADLG_Gesture" Guid="82614ed5-8978-4c2d-8771-aeab0b05a4e7" Variant="14" />
												</Commands>
											</Response>
										</Elements>
									</Sequence>
