									<Text Text="Nicméně...">
										<Localization Text="Nevertheless..." CreatedOn="2023-08-29T13:58:56.847997" UpdatedOn="2023-08-29T18:41:09.215532" Language="ENG" Translator="john.comer" />
										<Localization Text="Тем не менее..." CreatedOn="2023-08-30T10:13:44.186476" UpdatedOn="2023-09-07T18:08:24.669657" Language="RUS" Translator="tomas.vodslon" />
										<Localization Text="Niemniej..." CreatedOn="2023-08-30T10:27:26.326481" UpdatedOn="2023-09-07T19:00:24.013951" Language="POL" Translator="tomas.vodslon" />
										<Localization Text="Wie dem auch sei …" CreatedOn="2023-09-01T12:11:42.945797" UpdatedOn="2023-09-11T15:20:56.453471" Language="GER" Translator="tomas.vodslon" />
										<Localization Text="Bununla birlikte…" CreatedOn="2023-09-01T11:21:01.345918" UpdatedOn="2023-09-11T16:35:54.831682" Language="TUR" Translator="tomas.vodslon" />
										<Localization Text="In ogni modo…" CreatedOn="2023-09-11T17:56:48.832341" UpdatedOn="2023-09-11T18:03:01.124206" Language="ITA" Translator="tomas.vodslon" />
										<Localization Text="Quoi qu'il en soit…" CreatedOn="2023-09-11T19:44:25.08923" UpdatedOn="2023-09-11T19:45:31.80074" Language="FRA" Translator="tomas.vodslon" />
										<Localization Text="No obstante…" CreatedOn="2023-09-12T17:04:05.535807" UpdatedOn="2023-09-12T17:11:33.404823" Language="SPA" Translator="tomas.vodslon" />
										<Localization Text="De qualquer forma…" CreatedOn="2023-09-21T19:14:06.652743" UpdatedOn="2023-09-21T19:16:59.97612" Language="POR" Translator="tomas.vodslon" />
										<Localization Text="不过…" CreatedOn="2023-09-06T10:45:48.751699" UpdatedOn="2023-09-22T10:37:53.03822" Language="CHI" Translator="tomas.vodslon" />
										<Localization Text="とはいえ…" CreatedOn="2023-09-06T09:53:19.553842" UpdatedOn="2023-10-06T12:12:46.96543" Language="JPN" Translator="tomas.vodslon" />
										<Localization Text="Nicméně..." CreatedOn="2023-10-09T11:45:50.421925" UpdatedOn="2023-10-09T13:02:48.380006" Language="CZE" Translator="tomas.vodslon" />
										<Localization Text="不過呢……" CreatedOn="2023-09-06T10:47:14.265151" UpdatedOn="2023-10-16T11:39:15.345441" Language="ZHO" Translator="tomas.vodslon" />
										<Localization Text="어쨌든..." CreatedOn="2023-09-06T10:48:22.128205" UpdatedOn="2023-11-08T10:34:30.357907" Language="KOR" Translator="tomas.vodslon" />
										<Localization Text="Nicméně..." CreatedOn="2021-11-12T15:05:24.105983" UpdatedOn="2021-11-12T15:06:04.585518" Language="WHS" Translator="jonas.chmatal" />
										<Localization Text="Nevertheless..." CreatedOn="2022-05-09T22:27:36.733917" UpdatedOn="2022-05-13T17:32:49.511381" Language="PDG" Translator="john.comer" />
