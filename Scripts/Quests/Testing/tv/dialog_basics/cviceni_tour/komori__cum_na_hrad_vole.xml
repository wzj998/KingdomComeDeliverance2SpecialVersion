												<Text Text="Vítej na Troskách!">
													<Localization Text="Welcome to Trosky!" CreatedOn="2023-08-29T13:58:56.847997" UpdatedOn="2023-08-29T18:41:09.215532" Language="ENG" Translator="john.comer" />
													<Localization Text="Добро пожаловать в Троски!" CreatedOn="2023-08-30T10:13:44.186476" UpdatedOn="2023-09-07T18:08:24.669657" Language="RUS" Translator="tomas.vodslon" />
													<Localization Text="Witam na Troskach!" CreatedOn="2023-08-30T10:27:26.326481" UpdatedOn="2023-09-07T19:00:24.013951" Language="POL" Translator="tomas.vodslon" />
													<Localization Text="Willkommen in Trosky!" CreatedOn="2023-09-01T12:11:42.945797" UpdatedOn="2023-09-11T15:20:56.453471" Language="GER" Translator="tomas.vodslon" />
													<Localization Text="Trosky'e hoş geldiniz!" CreatedOn="2023-09-01T11:21:01.345918" UpdatedOn="2023-09-11T16:35:54.831682" Language="TUR" Translator="tomas.vodslon" />
													<Localization Text="Benvenuto a Trosky!" CreatedOn="2023-09-11T17:56:48.832341" UpdatedOn="2023-09-11T18:03:01.124206" Language="ITA" Translator="tomas.vodslon" />
													<Localization Text="Bienvenue à Trosky !" CreatedOn="2023-09-11T19:44:25.08923" UpdatedOn="2023-09-11T19:45:31.80074" Language="FRA" Translator="tomas.vodslon" />
													<Localization Text="¡Bienvenido a Trosky!" CreatedOn="2023-09-12T17:04:05.535807" UpdatedOn="2023-09-12T17:11:33.404823" Language="SPA" Translator="tomas.vodslon" />
													<Localization Text="Bem-vindo a Trosky!" CreatedOn="2023-09-21T19:14:06.652743" UpdatedOn="2023-09-21T19:16:59.97612" Language="POR" Translator="tomas.vodslon" />
													<Localization Text="欢迎来到托洛斯基！" CreatedOn="2023-09-06T10:45:48.751699" UpdatedOn="2023-09-22T10:37:53.03822" Language="CHI" Translator="tomas.vodslon" />
													<Localization Text="トロスキーへようこそ！" CreatedOn="2023-09-06T09:53:19.553842" UpdatedOn="2023-10-06T12:12:46.96543" Language="JPN" Translator="tomas.vodslon" />
													<Localization Text="Vítej na Troskách!" CreatedOn="2023-10-09T11:45:50.421925" UpdatedOn="2023-10-09T13:02:48.380006" Language="CZE" Translator="tomas.vodslon" />
													<Localization Text="還是歡迎你們來到特羅斯基！" CreatedOn="2023-09-06T10:47:14.265151" UpdatedOn="2023-10-16T11:39:15.345441" Language="ZHO" Translator="tomas.vodslon" />
													<Localization Text="트로스키에 온 걸 환영하네!" CreatedOn="2023-09-06T10:48:22.128205" UpdatedOn="2023-11-08T10:34:30.357907" Language="KOR" Translator="tomas.vodslon" />
													<Localization Text="Vítej na Troskách!" CreatedOn="2021-10-18T12:50:10.802077" UpdatedOn="2021-10-18T16:17:40.769287" Language="WHS" Translator="jonas.chmatal" />
													<Localization Text="Welcome to Trosky!" CreatedOn="2022-05-09T22:27:36.733917" UpdatedOn="2022-05-13T17:32:49.511381" Language="PDG" Translator="john.comer" />
