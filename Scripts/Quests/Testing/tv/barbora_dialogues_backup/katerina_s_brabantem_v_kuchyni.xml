<?xml version="1.0" encoding="utf-8"?>
<Database xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="brambora">
  <Skald>
    <Dialog Name="katerina_s_brabantem_v_kuchyni">
      <Text Text="Kateřina s Brabantem v kuchyni" />
      <Elements>
        <GameplayActionElement>
          <Text Text="Kateřina se otáčí v kuchyni, Brabant se opírá a vykecává" />
        </GameplayActionElement>
      </Elements>
      <Dialogue Type="ingame" LocalizationStatus="DontTranslate" Initiator="NonPlayer">
        <Decision Name="dec1" Priority="MainQuestSubmission">
          <Sequences>
            <Sequence EndType="Decision" Name="seq3">
              <Elements>
                <Response Role="BRABANT_GOSSIP" />
              </Elements>
              <Decision Name="dec2" Priority="MainQuestSubmission" Autoselect="true">
                <Sequences>
                  <Sequence EndType="EndDialogue" Cooldown="60s" Name="seq4">
                    <Elements>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="zamyšleně" />
                        </Parenthetical>
                        <Text Text="Kdybys jen tušila, ma chère, jak mi chybí domácká kuchyně...">
                          <Note StringName="kate_brabant_kdybys_jen_t_ZBPC_note" Text=" = má milá">
                            <Localization Text=" = má milá" CreatedOn="2023-05-23T16:11:51.28841" UpdatedOn="2023-05-23T16:22:46.507229" LoadedOn="2023-08-10T12:28:15.411713+02:00" Language="WHS" Translator="martina.danek" />
                          </Note>
                        </Text>
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="drobně jízlivě" />
                        </Parenthetical>
                        <Text Text="Copak? U nás ti nechutná?" />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="znalecky" />
                        </Parenthetical>
                        <Text Text="To ne, ale je to jako srovnávat... hmm... Mladé víno a obyčejný zkyslý mošt." />
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="odsekne" />
                        </Parenthetical>
                        <Text Text="Jestli chceš všechny ty žravý huby krmit sám, klidně ti udělám místo." />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="dotčeně se zakoktá" />
                        </Parenthetical>
                        <Text Text="Ale... Já... To ne...." />
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Text Text="Tak vidíš, pane barone. Moje bába vždycky říkala - doma jez co chceš, u cizích co ti daj." />
                      </Response>
                    </Elements>
                  </Sequence>
                  <Sequence EndType="EndDialogue" Cooldown="60s" Name="seq5">
                    <Elements>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="se zájmem" />
                        </Parenthetical>
                        <Text Text="Co si nechával servírovat k jídlu kardinál Pozzo na vinici?" />
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="věcně" />
                        </Parenthetical>
                        <Text Text="Vlastně samý prostý věci." />
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Text Text="Jahelnou kaši s medem k snídani, přes den si dal nějaký ovoce..." />
                        <ReviewNotesStylistic>
                          <Item Date="2023-07-04 15:21:29" UserName="david.horak" Text="oběd! Musí pryč." AcceptState="Accepted" />
                        </ReviewNotesStylistic>
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Text Text="Večer buď nic, nebo mrňavou číšku vína. A vůbec, jí dost skromně." />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="uznale" />
                        </Parenthetical>
                        <Text Text="To ho šlechtí. Musí jít příkladem... Tous les chrétiens.">
                          <Note StringName="kate_brabant_to_ho_slecht_ydCw_note" Text=" = všem křesťanům">
                            <Localization Text=" = všem křesťanům" CreatedOn="2023-05-23T16:56:38.1391" UpdatedOn="2023-05-23T17:21:50.093435" LoadedOn="2023-08-10T12:28:15.412713+02:00" Language="WHS" Translator="martina.danek" />
                          </Note>
                        </Text>
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="utrousí" />
                        </Parenthetical>
                        <Text Text="Zato chlapi z jeho družiny žerou jak prokopnutý..." />
                      </Response>
                    </Elements>
                  </Sequence>
                  <Sequence EndType="EndDialogue" Cooldown="60s" Name="seq6">
                    <Elements>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="bezelstně" />
                        </Parenthetical>
                        <Text Text="Ne, že bych tě nerada viděla, ale nebyl bys užitečnější někde jinde, barone?" />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="dotčeně" />
                        </Parenthetical>
                        <Text Text="Kdyby se po mně někdo sháněl, už bych o tom asi věděl, nemyslíš?" />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Text Text="Je to jejich škoda, že si celou tu l'aventure plánují sami, takříkajíc na koleni!">
                          <Note StringName="kate_brabant_je_to_jejich_rig4_note" Text=" = dobrodružství">
                            <Localization Text=" = dobrodružství" CreatedOn="2023-05-23T17:02:30.259763" UpdatedOn="2023-05-23T17:21:50.093435" LoadedOn="2023-08-10T12:28:15.412713+02:00" Language="WHS" Translator="martina.danek" />
                          </Note>
                        </Text>
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="věcně" />
                        </Parenthetical>
                        <Text Text="Jestli víš něco, co oni ne, měl bys jim to jít říct. Nevěřím, že by tě neposlechli." />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="uraženě" />
                        </Parenthetical>
                        <Text Text="Bylo mi jasně řečeno, že o mé rady a zkušenosti nestojí!" />
                      </Response>
                    </Elements>
                  </Sequence>
                  <Sequence EndType="EndDialogue" Cooldown="60s" Name="seq7">
                    <Elements>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="se zájmem" />
                        </Parenthetical>
                        <Text Text="Jak se vlastně vařilo u tebe doma, pane barone?" />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="zkouší ji" />
                        </Parenthetical>
                        <Text Text="Jak to myslíš? Vždyť já jsem doma skoro všude, ma chère. V Neapoli, Římě, Vídni, Praze...">
                          <Note StringName="kate_brabant_jak_to_mysli_LBya_note" Text="= má drahá">
                            <Localization Text="= má drahá" CreatedOn="2023-07-26T10:39:13.32139" UpdatedOn="2023-07-26T15:28:30.005074" LoadedOn="2023-08-10T12:28:15.7798019+02:00" Language="WHS" Translator="martina.danek" />
                          </Note>
                        </Text>
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="věcně" />
                        </Parenthetical>
                        <Text Text="No... prostě tam kdes přišel na svět." />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Parenthetical>
                          <Text Text="pochvalně" />
                        </Parenthetical>
                        <Text Text="Ach tak..." />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Text Text="Narodil jsem se u moře, takže jsme jedli hodně ryb. Tresky, štikozubce, plotice... vzácně i platýse." />
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Text Text="Co je to platýs?" />
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Text Text="To je zvláštní ryba. Žije na mořském dně a má oči jen na jedné straně hlavy. Obě dvě!">
                          <Note StringName="kate_brabant_to_je_druh_r_M6L9_note" Text=" = velmi bizarně">
                            <Localization Text=" = velmi bizarně" CreatedOn="2023-05-23T17:16:59.646004" UpdatedOn="2023-05-23T17:21:50.093435" LoadedOn="2023-08-10T12:28:15.412713+02:00" Language="WHS" Translator="martina.danek" />
                          </Note>
                        </Text>
                        <ReviewNotesStylistic>
                          <Item Date="2023-07-11 15:43:40" UserName="david.horak" Text="druh zní moc přírodovědecky" AcceptState="Accepted" />
                        </ReviewNotesStylistic>
                      </Response>
                      <Response Role="BRABANT_GOSSIP">
                        <Text Text="Vypadá... Ehm... Très bizarre." />
                      </Response>
                      <Response Role="KATERINA_GOSSIP">
                        <Parenthetical>
                          <Text Text="odtažitě" />
                        </Parenthetical>
                        <Text Text="Já slyšela o teleti, co se narodilo se dvěma hlavama... Ale jíst bych ho teda nechtěla..." />
                      </Response>
                    </Elements>
                  </Sequence>
                </Sequences>
              </Decision>
            </Sequence>
          </Sequences>
        </Decision>
      </Dialogue>
    </Dialog>
  </Skald>
