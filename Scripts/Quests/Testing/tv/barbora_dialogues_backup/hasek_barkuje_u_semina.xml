  <Skald>
    <Dialog Name="hasek_barkuje_u_semina">
      <Text Text="Hašek barkuje u Semína" />
      <Dialogue Type="ingame monolog" LocalizationStatus="DontTranslate" Initiator="NonPlayer" ClashPriority="Flavor">
        <Decision Name="dec1" Priority="MainQuestSubmission" Alias="mucirna_purkrabiBarkujeUSemina">
          <Sequences>
            <Sequence EndType="EndDialogue" Name="seq1">
              <Elements>
                <Response Role="PURKRABI_HASEK">
                  <Text Text="Už jsme skoro tam!">
                    <Localization Text="Už jsme skoro tam!" CreatedOn="2021-12-14T11:22:06.013238" UpdatedOn="2021-12-14T11:22:51.32095" Language="WHS" Translator="marek.cermak" />
                    <Localization Text="We're nearly there!" CreatedOn="2022-03-18T10:43:00.825917" UpdatedOn="2022-03-24T14:41:09.804333" Language="PDG" Translator="john.comer" />
                    <Localization Text="We're nearly there!" CreatedOn="2023-09-20T14:35:23.950651" UpdatedOn="2023-09-21T16:05:11.763063" Language="ENG" Translator="john.comer" />
                    <Localization Text="Siamo quasi arrivati!" CreatedOn="2023-10-03T17:26:45.884361" UpdatedOn="2023-10-03T17:29:10.368393" Language="ITA" Translator="ondrej.cerny" />
                    <Localization Text="¡Ya casi estamos!" CreatedOn="2023-10-03T18:00:09.273761" UpdatedOn="2023-10-03T18:02:39.580859" Language="SPA" Translator="ondrej.cerny" />
                    <Localization Text="Nous sommes presque arrivés !" CreatedOn="2023-10-03T18:08:54.387314" UpdatedOn="2023-10-03T18:10:00.052827" Language="FRA" Translator="ondrej.cerny" />
                    <Localization Text="Wir sind fast da!" CreatedOn="2023-09-22T11:23:19.681129" UpdatedOn="2023-10-04T16:55:27.607711" Language="GER" Translator="ondrej.cerny" />
                    <Localization Text="Neredeyse oradayız!" CreatedOn="2023-09-22T10:51:17.895499" UpdatedOn="2023-10-04T17:14:41.31634" Language="TUR" Translator="ondrej.cerny" />
                    <Localization Text="Почти на месте!" CreatedOn="2023-09-25T12:05:30.549789" UpdatedOn="2023-10-05T14:46:12.978375" Language="RUS" Translator="ondrej.cerny" />
                    <Localization Text="Już prawie na miejscu!" CreatedOn="2023-09-25T10:13:45.98558" UpdatedOn="2023-10-05T15:12:50.497241" Language="POL" Translator="ondrej.cerny" />
                    <Localization Text="Estamos quase lá!" CreatedOn="2023-10-06T18:56:20.399111" UpdatedOn="2023-10-06T19:00:53.062028" Language="POR" Translator="ondrej.cerny" />
                    <Localization Text="我们快到了！" CreatedOn="2023-09-26T13:58:12.311405" UpdatedOn="2023-10-24T09:53:06.377571" Language="CHI" Translator="ondrej.cerny" />
                    <Localization Text="もうすぐだ！" CreatedOn="2023-09-27T10:38:28.616816" UpdatedOn="2023-10-27T11:26:47.632316" Language="JPN" Translator="ondrej.cerny" />
                  </Text>
                </Response>
              </Elements>
            </Sequence>
          </Sequences>
        </Decision>
      </Dialogue>
    </Dialog>
  </Skald>
