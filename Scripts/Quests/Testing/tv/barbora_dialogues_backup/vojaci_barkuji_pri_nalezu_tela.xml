	<Skald>
		<Dialog Name="vojaci_barkuji_pri_nalezu_tela">
			<Text Text="Vojáci barkují při nálezu těla">
				<Localization Text="Vojáci barkují při nálezu těla" CreatedOn="2022-01-27T09:01:12.41572" UpdatedOn="2022-01-27T09:21:04.976658" Language="WHS" Translator="marek.cermak" />
				<Localization Text="Vojáci barkují při nálezu těla" CreatedOn="2023-11-04T23:06:17.116018" UpdatedOn="2023-11-04T23:14:32.5873" Language="CZE" Translator="ondrej.cerny" />
			</Text>
			<Dialogue Type="ingame monolog" LocalizationStatus="DontTranslate" Priority="1" Initiator="NonPlayer" ClashPriority="Flavor" GesturesNotNeeded="true">
				<Decision Name="dec1" Priority="MainQuestSubmission" Alias="mucirna_vojaciBarkujiPriNalezuTela" Autoselect="true">
					<Sequences>
						<Sequence EndType="EndDialogue" Name="seq2">
							<Elements>
								<Response Role="HASKUV_VOJAK_NA_SEMINE_NASEL_MRTVOLU">
									<Text Text="Kurva, to určitě udělal Oldřich!">
										<Localization Text="Kurva, to určitě udělal Oldřich!" CreatedOn="2022-01-27T09:01:28.868819" UpdatedOn="2022-01-27T09:21:05.142098" Language="WHS" Translator="marek.cermak" />
										<Localization Text="Fuck! It was Olda who did this, for sure!" CreatedOn="2022-03-18T10:43:00.825917" UpdatedOn="2022-03-28T12:15:42.882776" Language="PDG" Translator="john.comer" />
										<Localization Text="Fuck! It was Olda who did this, for sure!" CreatedOn="2023-09-20T14:35:23.950651" UpdatedOn="2023-09-21T16:35:25.457738" Language="ENG" Translator="john.comer" />
										<Localization Text="Merda! È stato Olda, di sicuro!" CreatedOn="2023-10-03T17:26:45.884361" UpdatedOn="2023-10-03T17:29:10.368393" Language="ITA" Translator="ondrej.cerny" />
										<Localization Text="¡Maldición! Seguro que Olda ha sido el responsable." CreatedOn="2023-10-03T18:00:09.273761" UpdatedOn="2023-10-03T18:02:39.580859" Language="SPA" Translator="ondrej.cerny" />
										<Localization Text="Merde ! C'est Olda qui a fait ça, c'est sûr !" CreatedOn="2023-10-03T18:08:54.387314" UpdatedOn="2023-10-03T18:10:00.052827" Language="FRA" Translator="ondrej.cerny" />
										<Localization Text="Scheiße! Olda hat das getan, ganz sicher!" CreatedOn="2023-09-22T11:23:19.681129" UpdatedOn="2023-10-04T16:55:27.607711" Language="GER" Translator="ondrej.cerny" />
										<Localization Text="Siktir! Bunu yapan kesinlikle Olda'ydı!" CreatedOn="2023-09-22T10:51:17.895499" UpdatedOn="2023-10-04T17:14:41.31634" Language="TUR" Translator="ondrej.cerny" />
										<Localization Text="Курва! Это точно дело рук Ольдржиха!" CreatedOn="2023-09-25T12:05:30.549789" UpdatedOn="2023-10-05T14:46:12.978375" Language="RUS" Translator="ondrej.cerny" />
										<Localization Text="Kurwa! To na pewno robota Oldrzycha!" CreatedOn="2023-09-25T10:13:45.98558" UpdatedOn="2023-10-05T15:12:50.497241" Language="POL" Translator="ondrej.cerny" />
										<Localization Text="Porra! Foi Olda quem fez isso, com certeza!" CreatedOn="2023-10-06T18:56:20.399111" UpdatedOn="2023-10-06T19:00:53.062028" Language="POR" Translator="ondrej.cerny" />
										<Localization Text="妈的！肯定是奥达干的！" CreatedOn="2023-09-26T13:58:12.311405" UpdatedOn="2023-10-24T09:53:06.377571" Language="CHI" Translator="ondrej.cerny" />
										<Localization Text="クソ！ オルダの仕業に違いない！" CreatedOn="2024-03-28T10:15:58.750047" UpdatedOn="2024-03-28T11:03:48.215072" Language="JPN" Translator="ondrej.cerny" />
										<Localization Text="Kurva! To určitě udělal Oldřich!" CreatedOn="2023-11-04T23:06:17.116018" UpdatedOn="2023-11-04T23:14:32.5873" Language="CZE" Translator="ondrej.cerny" />
										<Localization Text="媽的！一定是歐達幹的好事！" CreatedOn="2023-09-26T14:09:45.61721" UpdatedOn="2023-11-08T09:03:41.623324" Language="ZHO" Translator="ondrej.cerny" />
										<Localization Text="썅! 분명히 올다가 한 짓이야!" CreatedOn="2023-09-26T14:08:36.457583" UpdatedOn="2023-11-29T10:10:42.865091" Language="KOR" Translator="ondrej.cerny" />
									</Text>
								</Response>
							</Elements>
						</Sequence>
						<Sequence EndType="EndDialogue" Name="seq3">
							<Elements>
								<Response Role="HASKUV_VOJAK_NA_SEMINE_NASEL_MRTVOLU">
									<Text Text="Ten sráč Semín tady někde musí bejt!">
										<Localization Text="Ten sráč Semín tady někde musí bejt!" CreatedOn="2022-01-27T09:04:05.167967" UpdatedOn="2022-01-27T09:21:05.307635" Language="WHS" Translator="marek.cermak" />
										<Localization Text="That piece of shit Semine must be here somewhere!" CreatedOn="2022-03-18T10:43:00.825917" UpdatedOn="2022-03-28T12:15:42.882776" Language="PDG" Translator="john.comer" />
										<Localization Text="That piece of shit Semine must be here somewhere!" CreatedOn="2023-09-20T14:35:23.950651" UpdatedOn="2023-09-21T16:35:25.457738" Language="ENG" Translator="john.comer" />
										<Localization Text="Quello sciagurato del giovane Olda di Semine dev'essere qui!" CreatedOn="2023-10-03T17:26:45.884361" UpdatedOn="2023-10-03T17:29:10.368393" Language="ITA" Translator="ondrej.cerny" />
										<Localization Text="¡Esa escoria de Semín debe de andar cerca!" CreatedOn="2023-10-03T18:00:09.273761" UpdatedOn="2023-10-03T18:02:39.580859" Language="SPA" Translator="ondrej.cerny" />
										<Localization Text="Cette raclure de Semine doit être ici, quelque part !" CreatedOn="2023-10-03T18:08:54.387314" UpdatedOn="2023-10-03T18:10:00.052827" Language="FRA" Translator="ondrej.cerny" />
										<Localization Text="Dieses Drecksschwein Semin muss hier irgendwo sein!" CreatedOn="2023-09-22T11:23:19.681129" UpdatedOn="2023-10-04T16:55:27.607711" Language="GER" Translator="ondrej.cerny" />
										<Localization Text="O bok parçası Semine buralarda bir yerde olmalı!" CreatedOn="2023-09-22T10:51:17.895499" UpdatedOn="2023-10-04T17:14:41.31634" Language="TUR" Translator="ondrej.cerny" />
										<Localization Text="Этот сраный панич где-то здесь!" CreatedOn="2023-09-25T12:05:30.549789" UpdatedOn="2023-10-05T14:46:12.978375" Language="RUS" Translator="ondrej.cerny" />
										<Localization Text="Ten zasrany Semin musi gdzieś tu być!" CreatedOn="2023-09-25T10:13:45.98558" UpdatedOn="2023-10-05T15:12:50.497241" Language="POL" Translator="ondrej.cerny" />
										<Localization Text="Aquele merda do Semine deve estar aqui em algum lugar!" CreatedOn="2023-10-06T18:56:20.399111" UpdatedOn="2023-10-06T19:00:53.062028" Language="POR" Translator="ondrej.cerny" />
										<Localization Text="那个塞米混蛋肯定在这里！" CreatedOn="2023-09-26T13:58:12.311405" UpdatedOn="2023-10-24T09:53:06.377571" Language="CHI" Translator="ondrej.cerny" />
										<Localization Text="あのセミンのクソ野郎はこのあたりにいるはずだ！" CreatedOn="2023-09-27T10:38:28.616816" UpdatedOn="2023-10-27T11:26:47.632316" Language="JPN" Translator="ondrej.cerny" />
										<Localization Text="Ten sráč Semín tady někde musí bejt!" CreatedOn="2023-11-04T23:06:17.116018" UpdatedOn="2023-11-04T23:14:32.5873" Language="CZE" Translator="ondrej.cerny" />
										<Localization Text="那該死的瑟米涅混蛋一定就在這附近！" CreatedOn="2023-09-26T14:09:45.61721" UpdatedOn="2023-11-08T09:03:41.623324" Language="ZHO" Translator="ondrej.cerny" />
										<Localization Text="그 쓰레기 같은 세민 놈이 분명 여기 있을 거다!" CreatedOn="2023-09-26T14:08:36.457583" UpdatedOn="2023-11-29T10:10:42.865091" Language="KOR" Translator="ondrej.cerny" />
									</Text>
								</Response>
							</Elements>
						</Sequence>
						<Sequence EndType="EndDialogue" Name="seq4">
							<Elements>
								<Response Role="HASKUV_VOJAK_NA_SEMINE_NASEL_MRTVOLU">
									<Text Text="Doprdele, Oldřich nás sejme všechny!">
										<Localization Text="Doprdele, Oldřich nás sejme všechny!" CreatedOn="2022-01-27T09:16:49.503335" UpdatedOn="2022-01-27T09:21:05.469865" Language="WHS" Translator="marek.cermak" />
										<Localization Text="Shit! That Olda will kill us all!" CreatedOn="2022-03-18T10:43:00.825917" UpdatedOn="2022-03-28T12:15:42.882776" Language="PDG" Translator="john.comer" />
										<Localization Text="Shit! That Olda will kill us all!" CreatedOn="2023-09-20T14:35:23.950651" UpdatedOn="2023-09-21T16:35:25.457738" Language="ENG" Translator="john.comer" />
										<Localization Text="Merda! Quell'Olda ci ucciderà tutti!" CreatedOn="2023-10-03T17:26:45.884361" UpdatedOn="2023-10-03T17:29:10.368393" Language="ITA" Translator="ondrej.cerny" />
										<Localization Text="¡Condenado sea Olda, nos matará a todos!" CreatedOn="2023-10-03T18:00:09.273761" UpdatedOn="2023-10-03T18:02:39.580859" Language="SPA" Translator="ondrej.cerny" />
										<Localization Text="Merde ! Cet Olda nous tuera tous !" CreatedOn="2023-10-03T18:08:54.387314" UpdatedOn="2023-10-03T18:10:00.052827" Language="FRA" Translator="ondrej.cerny" />
										<Localization Text="Scheiße! Olda bringt uns noch alle um!" CreatedOn="2023-09-22T11:23:19.681129" UpdatedOn="2023-10-04T16:55:27.607711" Language="GER" Translator="ondrej.cerny" />
										<Localization Text="Sıçtık! O Olda hepimizi öldürecek!" CreatedOn="2023-09-22T10:51:17.895499" UpdatedOn="2023-10-04T17:14:41.31634" Language="TUR" Translator="ondrej.cerny" />
										<Localization Text="Чёрт! Этот Ольдржих всех нас перережет!" CreatedOn="2023-09-25T12:05:30.549789" UpdatedOn="2023-10-05T14:46:12.978375" Language="RUS" Translator="ondrej.cerny" />
										<Localization Text="Kurwa! Oldrzych nas wszystkich powybija!" CreatedOn="2023-09-25T10:13:45.98558" UpdatedOn="2023-10-05T15:12:50.497241" Language="POL" Translator="ondrej.cerny" />
										<Localization Text="Merda! Olda vai matar todos nós!" CreatedOn="2023-10-06T18:56:20.399111" UpdatedOn="2023-10-06T19:00:53.062028" Language="POR" Translator="ondrej.cerny" />
										<Localization Text="妈的！那个奥达会杀了我们所有人的！" CreatedOn="2023-09-26T13:58:12.311405" UpdatedOn="2023-10-24T09:53:06.377571" Language="CHI" Translator="ondrej.cerny" />
										<Localization Text="クソ！ オルダに全員殺される！" CreatedOn="2023-09-27T10:38:28.616816" UpdatedOn="2023-10-27T11:26:47.632316" Language="JPN" Translator="ondrej.cerny" />
										<Localization Text="Do prdele, Oldřich nás sejme všechny!" CreatedOn="2023-11-04T23:06:17.116018" UpdatedOn="2023-11-04T23:14:32.5873" Language="CZE" Translator="ondrej.cerny" />
										<Localization Text="媽的！歐達會把我們通通害死的！" CreatedOn="2023-09-26T14:09:45.61721" UpdatedOn="2023-11-08T09:03:41.623324" Language="ZHO" Translator="ondrej.cerny" />
										<Localization Text="젠장! 올다라는 놈에게 죄다 당하겠어!" CreatedOn="2023-09-26T14:08:36.457583" UpdatedOn="2023-11-29T10:10:42.865091" Language="KOR" Translator="ondrej.cerny" />
									</Text>
								</Response>
							</Elements>
						</Sequence>
						<Sequence EndType="EndDialogue" Name="seq5">
							<Elements>
								<Response Role="HASKUV_VOJAK_NA_SEMINE_NASEL_MRTVOLU">
									<Text Text="Ten zmrd se nám mstí...">
										<Localization Text="Ten zmrd se nám mstí..." CreatedOn="2022-01-27T09:18:00.523553" UpdatedOn="2022-01-27T09:21:05.630767" Language="WHS" Translator="marek.cermak" />
										<Localization Text="That bastard is getting revenge on us!" CreatedOn="2022-03-18T10:43:00.825917" UpdatedOn="2022-03-28T12:15:42.882776" Language="PDG" Translator="john.comer" />
										<Localization Text="That bastard is getting revenge on us!" CreatedOn="2023-09-20T14:35:23.950651" UpdatedOn="2023-09-21T16:35:25.457738" Language="ENG" Translator="john.comer" />
										<Localization Text="Quel bastardo si vendicherà di noi!" CreatedOn="2023-10-03T17:26:45.884361" UpdatedOn="2023-10-03T17:29:10.368393" Language="ITA" Translator="ondrej.cerny" />
										<Localization Text="¡Ese desgraciado se está vengando de nosotros!" CreatedOn="2023-10-03T18:00:09.273761" UpdatedOn="2023-10-03T18:02:39.580859" Language="SPA" Translator="ondrej.cerny" />
										<Localization Text="Ce fumier se venge de nous !" CreatedOn="2023-10-03T18:08:54.387314" UpdatedOn="2023-10-03T18:10:00.052827" Language="FRA" Translator="ondrej.cerny" />
										<Localization Text="Der Bastard will sich an uns rächen!" CreatedOn="2023-09-22T11:23:19.681129" UpdatedOn="2023-10-04T16:55:27.607711" Language="GER" Translator="ondrej.cerny" />
										<Localization Text="O piç bizden intikam alıyor!" CreatedOn="2023-09-22T10:51:17.895499" UpdatedOn="2023-10-04T17:14:41.31634" Language="TUR" Translator="ondrej.cerny" />
										<Localization Text="Этот ублюдок нам мстит!" CreatedOn="2023-09-25T12:05:30.549789" UpdatedOn="2023-10-05T14:46:12.978375" Language="RUS" Translator="ondrej.cerny" />
										<Localization Text="Ten drań się na nas mści!" CreatedOn="2023-09-25T10:13:45.98558" UpdatedOn="2023-10-05T15:12:50.497241" Language="POL" Translator="ondrej.cerny" />
										<Localization Text="O desgraçado está se vingando na gente!" CreatedOn="2023-10-06T18:56:20.399111" UpdatedOn="2023-10-06T19:00:53.062028" Language="POR" Translator="ondrej.cerny" />
										<Localization Text="那个杂碎在向我们报仇！" CreatedOn="2023-09-26T13:58:12.311405" UpdatedOn="2023-10-24T09:53:06.377571" Language="CHI" Translator="ondrej.cerny" />
										<Localization Text="あの野郎 俺たちに復讐してやがる！" CreatedOn="2024-03-28T10:15:58.750047" UpdatedOn="2024-03-28T11:03:48.215072" Language="JPN" Translator="ondrej.cerny" />
										<Localization Text="Ten zmrd se nám mstí!" CreatedOn="2023-11-04T23:06:17.116018" UpdatedOn="2023-11-04T23:14:32.5873" Language="CZE" Translator="ondrej.cerny" />
										<Localization Text="這混蛋想報復我們！" CreatedOn="2023-09-26T14:09:45.61721" UpdatedOn="2023-11-08T09:03:41.623324" Language="ZHO" Translator="ondrej.cerny" />
										<Localization Text="그 개자식이 반격하기 시작했다!" CreatedOn="2023-09-26T14:08:36.457583" UpdatedOn="2023-11-29T10:10:42.865091" Language="KOR" Translator="ondrej.cerny" />
									</Text>
								</Response>
							</Elements>
						</Sequence>
						<Sequence EndType="EndDialogue" Name="seq1">
							<Elements>
								<Response Role="HASKUV_VOJAK_NA_SEMINE_NASEL_MRTVOLU">
									<Text Text="Za tohle zaplatíš, Oldřichu!">
										<Localization Text="You'll pay for this, Olda!" CreatedOn="2022-03-18T10:43:00.825917" UpdatedOn="2022-03-28T12:15:42.882776" Language="PDG" Translator="john.comer" />
										<Localization Text="Za tohle zaplatíš, Oldřichu!" CreatedOn="2022-01-27T09:20:00.955227" UpdatedOn="2023-07-27T14:00:15.175691" Language="WHS" Translator="richard.malatinsky" />
										<Localization Text="You'll pay for this, Olda!" CreatedOn="2023-09-20T14:35:23.950651" UpdatedOn="2023-09-21T16:35:25.457738" Language="ENG" Translator="john.comer" />
										<Localization Text="La pagherai, Olda!" CreatedOn="2023-10-03T17:26:45.884361" UpdatedOn="2023-10-03T17:29:10.368393" Language="ITA" Translator="ondrej.cerny" />
										<Localization Text="¡Pagaréis por esto, Olda!" CreatedOn="2023-10-03T18:00:09.273761" UpdatedOn="2023-10-03T18:02:39.580859" Language="SPA" Translator="ondrej.cerny" />
										<Localization Text="Tu paieras pour ça, Olda !" CreatedOn="2023-10-03T18:08:54.387314" UpdatedOn="2023-10-03T18:10:00.052827" Language="FRA" Translator="ondrej.cerny" />
										<Localization Text="Dafür bezahlst du, Olda!" CreatedOn="2023-09-22T11:23:19.681129" UpdatedOn="2023-10-04T16:55:27.607711" Language="GER" Translator="ondrej.cerny" />
										<Localization Text="Bunu ödeyeceksin, Olda!" CreatedOn="2023-09-22T10:51:17.895499" UpdatedOn="2023-10-04T17:14:41.31634" Language="TUR" Translator="ondrej.cerny" />
										<Localization Text="Ты поплатишься за это, Ольдржих!" CreatedOn="2023-09-25T12:05:30.549789" UpdatedOn="2023-10-05T14:46:12.978375" Language="RUS" Translator="ondrej.cerny" />
										<Localization Text="Zapłacisz za to, Oldrzychu!" CreatedOn="2023-09-25T10:13:45.98558" UpdatedOn="2023-10-05T15:12:50.497241" Language="POL" Translator="ondrej.cerny" />
										<Localization Text="Você vai pagar por isso, Olda!" CreatedOn="2023-10-06T18:56:20.399111" UpdatedOn="2023-10-06T19:00:53.062028" Language="POR" Translator="ondrej.cerny" />
										<Localization Text="你会为此付出代价的，奥达！" CreatedOn="2023-09-26T13:58:12.311405" UpdatedOn="2023-10-24T09:53:06.377571" Language="CHI" Translator="ondrej.cerny" />
										<Localization Text="報いを受けろ オルダ！" CreatedOn="2024-03-28T10:15:58.750047" UpdatedOn="2024-03-28T11:03:48.215072" Language="JPN" Translator="ondrej.cerny" />
										<Localization Text="Za tohle zaplatíš, Oldřichu!" CreatedOn="2023-11-04T23:06:17.116018" UpdatedOn="2023-11-04T23:14:32.5873" Language="CZE" Translator="ondrej.cerny" />
										<Localization Text="你會為此付出代價的，歐達！" CreatedOn="2023-09-26T14:09:45.61721" UpdatedOn="2023-11-08T09:03:41.623324" Language="ZHO" Translator="ondrej.cerny" />
										<Localization Text="가만두지 않겠다, 올다!" CreatedOn="2023-09-26T14:08:36.457583" UpdatedOn="2023-11-29T10:10:42.865091" Language="KOR" Translator="ondrej.cerny" />
									</Text>
								</Response>
							</Elements>
						</Sequence>
					</Sequences>
				</Decision>
			</Dialogue>
		</Dialog>
	</Skald>
