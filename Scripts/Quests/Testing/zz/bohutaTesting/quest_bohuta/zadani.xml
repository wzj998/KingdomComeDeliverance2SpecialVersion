<?xml version="1.0" encoding="utf-8"?>
<Database xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="brambora">
  <Skald>
    <Dialog Name="zadani">
      <Ports>
        <Port Name="startquest" Direction="Out" Type="trigger">
          <DesignName Text="StartQuest" />
        </Port>
        <Port Name="questavailable" Direction="In" Type="bool">
          <DesignName Text="questAvailable" />
        </Port>
      </Ports>
      <Text Text="Zadani" />
      <Dialogue TechnicalStatus="Enabled">
        <Decision Name="dec1">
          <Sequences>
            <Sequence EntryCondition="Port('questavailable')" EndType="Decision" Name="seq2">
              <UiPrompt Text="Bohuta: začátek" />
              <Elements>
                <Response Role="BOHUTA_PLAYER">
                  <Commands>
                    <CameraCommand />
                  </Commands>
                </Response>
                <Response Role="TEST_BOHUTA_NPC_4">
                  <Text Text="Přines mi jabko." />
                  <Commands>
                    <CameraCommand />
                  </Commands>
                </Response>
              </Elements>
              <Decision Name="dec2">
                <Sequences>
                  <Sequence EndType="EndDialogue" Name="seq3">
                    <UiPrompt Text="ok" />
                    <Triggers>
                      <Port Name="startquest" />
                    </Triggers>
                    <Elements>
                      <Response Role="BOHUTA_PLAYER">
                        <Text Text="Ok. Jdu na to." />
                        <Commands>
                          <CameraCommand />
                        </Commands>
                      </Response>
                    </Elements>
                  </Sequence>
                  <Sequence EndType="EndDialogue" Name="seq5">
                    <UiPrompt Text="jindy" />
                    <Elements>
                      <Response Role="BOHUTA_PLAYER">
                        <Text Text="Nemam čas." />
                        <Commands>
                          <CameraCommand />
                        </Commands>
                      </Response>
                    </Elements>
                  </Sequence>
                </Sequences>
              </Decision>
            </Sequence>
          </Sequences>
        </Decision>
      </Dialogue>
    </Dialog>
  </Skald>
</Database>