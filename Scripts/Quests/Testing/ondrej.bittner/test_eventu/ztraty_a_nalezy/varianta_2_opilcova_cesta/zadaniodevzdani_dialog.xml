<?xml version="1.0" encoding="utf-8"?>
<Database xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="brambora">
  <Skald>
    <Dialog Name="zadaniodevzdani_dialog">
      <Text Text="Zadání/Odevzdání dialog" />
      <Dialogue AllowGreeting="false">
        <Decision Name="dec1">
          <Sequences>
            <Sequence Name="seq2" EndType="Decision">
              <Decision Name="dec2">
                <Sequences>
                  <Sequence Name="seq4" EndType="EndDialogue">
                    <Elements>
                      <SequenceElementWithRole Role="HENRY">
                        <Lines>
                          <Response>
                            <Text Text="Můžu ti pomoct hledat." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                        <Lines>
                          <Response>
                            <Text Text="To by si mi fakt píchnul. Já se snažim, ale jestli se rozejdu někam dál, tak se bojim, že to v sobě neudržim." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="HENRY">
                        <Lines>
                          <Response>
                            <Text Text="Jasný. To chápu no a co všechno postrádáš kromě důstojnosti. S tou se bojim ti nepomůžu." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                        <Lines>
                          <Response>
                            <Text Text="No čepec, takovej oranžovej. Zelenou tuniku sem měl. Asi teda. A boty ... obyčejný kožený." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="HENRY">
                        <Lines>
                          <Response>
                            <Text Text="Dobře tak já se tu porozhlídnu." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                        <Lines>
                          <Response>
                            <Text Text="Jo díky." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                    </Elements>
                  </Sequence>
                  <Sequence Name="seq3" EndType="EndDialogue">
                    <Elements>
                      <SequenceElementWithRole Role="HENRY">
                        <Lines>
                          <Response>
                            <Text Text="No to je blbý. Bejt tebou už bych tolik nepil." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                        <Lines>
                          <Response>
                            <Text Text="Eh ... no já už tolik nepiju. Snažím se mírnit." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="HENRY">
                        <Lines>
                          <Response>
                            <Text Text="Aha ... no v tom případě. Hodně štěstí." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                        <Lines>
                          <Response>
                            <Text Text="Jo ... díky  ..." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                    </Elements>
                  </Sequence>
                </Sequences>
              </Decision>
              <Elements>
                <SequenceElementWithRole Role="HENRY">
                  <Lines>
                    <Response>
                      <Text Text="Ty tu něco hledáš pane?" />
                    </Response>
                  </Lines>
                </SequenceElementWithRole>
                <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                  <Lines>
                    <Response>
                      <Text Text="Uhhh ... no jo hledám. Ty jo člověče ty mi jdeš zrovna do cesty." />
                    </Response>
                  </Lines>
                </SequenceElementWithRole>
                <SequenceElementWithRole Role="HENRY">
                  <Lines>
                    <Response>
                      <Text Text="Aha jakto?" />
                    </Response>
                  </Lines>
                </SequenceElementWithRole>
                <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                  <Lines>
                    <Response>
                      <Text Text="No ... sem se včera nějak máznul a teď sem se probudil. Kousek támhle za cestou ..." />
                    </Response>
                  </Lines>
                </SequenceElementWithRole>
                <SequenceElementWithRole Role="HENRY">
                  <Lines>
                    <Response>
                      <Text Text="A nemáš svý věci." />
                    </Response>
                  </Lines>
                </SequenceElementWithRole>
                <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                  <Lines>
                    <Response>
                      <Text Text="No ... nohavice mám ... ale zbytek ... je fuč. Včetně důstojnosti." />
                    </Response>
                  </Lines>
                </SequenceElementWithRole>
              </Elements>
            </Sequence>
            <Sequence Name="seq5" EndType="Decision">
              <Decision Name="dec3">
                <Sequences>
                  <Sequence Name="seq6" EndType="EndDialogue">
                    <Elements>
                      <SequenceElementWithRole Role="HENRY">
                        <Lines>
                          <Response>
                            <Text Text="Nemám. Seš si jistej, že to je tady někde kolem?" />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                        <Lines>
                          <Response>
                            <Text Text="Jistej? Člověče já ani nevim co je za den v tejdnu." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="HENRY">
                        <Lines>
                          <Response>
                            <Text Text="No nic ... kouknu dál." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                    </Elements>
                  </Sequence>
                  <Sequence Name="seq7" EndType="Decision">
                    <Decision Name="dec4">
                      <Sequences>
                        <Sequence Name="seq8" EndType="EndDialogue">
                          <Elements>
                            <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                              <Lines>
                                <Response>
                                  <Text Text="Díky ti moc člověče. A tady si vem něco na cesty. Já dneska asi stejně nebudu mít chuť ... na pití." />
                                </Response>
                              </Lines>
                            </SequenceElementWithRole>
                            <SequenceElementWithRole Role="HENRY">
                              <Lines>
                                <Response>
                                  <Text Text="Tak dík. A dávej pozor!" />
                                </Response>
                              </Lines>
                            </SequenceElementWithRole>
                            <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                              <Lines>
                                <Response>
                                  <Text Text="Neboj, budu!" />
                                </Response>
                              </Lines>
                            </SequenceElementWithRole>
                          </Elements>
                        </Sequence>
                        <Sequence Name="seq9" EndType="EndDialogue">
                          <Elements>
                            <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                              <Lines>
                                <Response>
                                  <Text Text="To je všecho?" />
                                </Response>
                              </Lines>
                            </SequenceElementWithRole>
                            <SequenceElementWithRole Role="HENRY">
                              <Lines>
                                <Response>
                                  <Text Text="Zatím jo ... ještě kouknu po zbytku." />
                                </Response>
                              </Lines>
                            </SequenceElementWithRole>
                          </Elements>
                        </Sequence>
                      </Sequences>
                    </Decision>
                    <Elements>
                      <SequenceElementWithRole Role="HENRY">
                        <Lines>
                          <Response>
                            <Text Text="Našel, našel." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                      <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                        <Lines>
                          <Response>
                            <Text Text="Ha! Sem zachráněn - tak ukaž." />
                          </Response>
                        </Lines>
                      </SequenceElementWithRole>
                    </Elements>
                  </Sequence>
                </Sequences>
              </Decision>
              <Elements>
                <SequenceElementWithRole Role="EVENT_ZTRATY_A_NALEZY_-_MUZ">
                  <Lines>
                    <Response>
                      <Text Text="Tak co máš?" />
                    </Response>
                  </Lines>
                </SequenceElementWithRole>
              </Elements>
            </Sequence>
          </Sequences>
        </Decision>
      </Dialogue>
    </Dialog>
  </Skald>
</Database>