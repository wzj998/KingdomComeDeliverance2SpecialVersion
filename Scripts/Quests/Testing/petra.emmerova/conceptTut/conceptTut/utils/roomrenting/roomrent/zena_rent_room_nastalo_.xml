                                <CameraCommand />
                              </Commands>
                            </Response>
                            <Response Role="HENRY">
                              <Parenthetical>
                                <Text Text="zakroutí hlavou, gesto rukou" />
                              </Parenthetical>
                              <Text Text="Ne, zaplatím ho na stálo." />
