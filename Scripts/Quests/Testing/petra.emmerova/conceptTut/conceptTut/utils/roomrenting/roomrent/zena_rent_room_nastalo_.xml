                                <CameraCommand />
                              </Commands>
                            </Response>
                          </Elements>
                          <Decision Name="Bribe">
                            <Sequences>
                              <Sequence ExitScript="SetupBribe(500)" EndType="Decision" Visibility="Internal" Type="Bribe" Name="bribe">
                                <DesignName Text="Bribe" />
