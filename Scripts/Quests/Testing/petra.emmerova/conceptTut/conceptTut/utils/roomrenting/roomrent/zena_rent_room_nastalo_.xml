                                                  <Text Text="pokrčí rameny" />
                                                </Parenthetical>
                                                <Text Text="To nejde, bohužel." />
                                                <Commands>
                                                  <CameraCommand />
                                                </Commands>
                                              </Response>
                                            </Elements>
                                          </Sequence>
                                        </Sequences>
                                      </Decision>
                                    </Sequence>
                                  </Sequences>
                                </Decision>
                              </Sequence>
                            </Sequences>
                          </Decision>
