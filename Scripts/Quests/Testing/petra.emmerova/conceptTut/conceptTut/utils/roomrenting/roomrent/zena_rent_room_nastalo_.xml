        <GameplayActionElement>
          <Text Text="Dialog pro ženy - pro šenkýřky a hospodské." />
        </GameplayActionElement>
      </Elements>
      <Dialogue>
        <Decision Name="dec1">
          <Sequences>
            <Sequence EntryCondition="Port('pronajem_na_jednu_noc_active')" EndType="Decision" Name="seq12">
