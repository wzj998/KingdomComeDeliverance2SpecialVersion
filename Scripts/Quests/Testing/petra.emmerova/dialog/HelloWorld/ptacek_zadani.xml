<?xml version="1.0" encoding="utf-8"?>
<Database xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="brambora">
  <Skald>
    <FaderDialog Name="ptacek_zadani">
      <Text Text="Ptáček zadání" />
      <Dialogue TechnicalStatus="Enabled">
        <Decision Name="dec1">
          <Sequences>
            <Sequence EntryCondition="!ThisSequenceUsed()" EndType="Decision" Name="seq1">
              <UiPrompt Text="Jak to jde?" />
              <Elements>
                <Response Role="HENRY">
                  <Text Text="Jak to jde?" />
                  <Commands>
                    <CameraCommand />
                  </Commands>
                </Response>
                <Response Role="PTACEK">
                  <Text Text="Jsem naštvanej. Ztratil jsem meč." />
                  <Commands>
                    <CameraCommand />
                  </Commands>
                </Response>
              </Elements>
              <Decision Name="dec2">
                <Sequences>
                  <Sequence EndType="Decision" Name="seq2">
                    <UiPrompt Text="Podívám se po něm" />
                    <Elements>
                      <Response Role="HENRY">
                        <Text Text="Podívám se po něm." />
                        <Commands>
                          <CameraCommand />
                        </Commands>
                      </Response>
                    </Elements>
                    <Decision Name="dec3">
                      <Sequences>
                        <Sequence EndType="EndDialogue" Name="seq5">
                          <Elements>
                            <Response Role="PTACEK">
                              <Text Text="Bezvadný, díky." />
                              <Commands>
                                <CameraCommand />
                              </Commands>
                            </Response>
                            <Response Role="PTACEK">
                              <Text Text="Myslím že jsem ho musel ztratit na tržišti u radnice." />
                            </Response>
                            <Response Role="HENRY">
                              <Text Text="Podívám se tam." />
                              <Commands>
                                <CameraCommand />
                              </Commands>
                            </Response>
                          </Elements>
                        </Sequence>
                      </Sequences>
                    </Decision>
                  </Sequence>
                  <Sequence EndType="EndDialogue" Name="seq3">
                    <UiPrompt Text="Nemám čas se tím teď zabývat" />
                    <Elements>
                      <Response Role="HENRY">
                        <Text Text="Nemám čas se tím teď zabývat" />
                        <Commands>
                          <CameraCommand />
                        </Commands>
                      </Response>
                      <Response Role="HENRY">
                        <Text Text="Beztak by sis akorát vypích voko." />
                      </Response>
                      <Response Role="PTACEK">
                        <Text Text="Nemám tě rád, mlékaři Jindro." />
                        <Commands>
                          <CameraCommand />
                        </Commands>
                      </Response>
                      <Response Role="PTACEK">
                        <Text Text="Ale kdyby sis to rozmyslel, přijď." />
                      </Response>
                    </Elements>
                  </Sequence>
                </Sequences>
              </Decision>
            </Sequence>
            <Sequence EndType="Decision" Name="seq4">
              <UiPrompt Text="Hledáš ještě ten meč?" />
              <Elements>
                <Response Role="HENRY">
                  <Text Text="Koukám že máš obě oči, tak tos asi ještě ten meč nenašel, co?" />
                  <Commands>
                    <CameraCommand />
                  </Commands>
                </Response>
                <Response Role="PTACEK">
                  <Text Text="Nenašel..." />
                  <Commands>
                    <CameraCommand />
                  </Commands>
                </Response>
              </Elements>
              <Decision Name="dec4">
                <Sequences>
                  <Sequence EndType="EndDialogue" Name="seq7">
                    <UiPrompt Text="To máš blbý no" />
                    <Elements>
                      <Response Role="HENRY">
                        <Text Text="To máš blbý no." />
                        <Commands>
                          <CameraCommand />
                        </Commands>
                      </Response>
                      <Response Role="PTACEK">
                        <Text Text="Jindro, víš, občas si říkám proč s tebou vůbec kamarádím." />
                        <Commands>
                          <CameraCommand />
                        </Commands>
                      </Response>
                    </Elements>
                  </Sequence>
                  <Sequence EndType="GoTo" GoToDecision="dec3" SpeechCoef="0" Name="seq6">
                    <UiPrompt Text="Tak já se po něm teda podívám." />
                    <Elements>
                      <Response Role="HENRY">
                        <Text Text="Tak já se po něm teda podívám." />
                        <Commands>
                          <CameraCommand />
                        </Commands>
                      </Response>
                    </Elements>
                  </Sequence>
                </Sequences>
              </Decision>
            </Sequence>
          </Sequences>
        </Decision>
      </Dialogue>
    </FaderDialog>
  </Skald>
