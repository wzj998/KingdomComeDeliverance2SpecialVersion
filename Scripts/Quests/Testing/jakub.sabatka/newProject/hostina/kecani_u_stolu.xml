<?xml version="1.0" encoding="utf-8"?>
<Database xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="brambora">
  <Skald>
    <Dialog Name="kecani_u_stolu">
      <Text Text="Kecání u stolu" />
      <Dialogue Type="ingame" TechnicalStatus="Enabled" Initiator="NonPlayer" LocalizationStatus="DontTranslate">
        <Decision Name="dec1" Alias="test_jakub_kecaniUStolu">
          <Sequences>
            <Sequence EndType="EndDialogue" Name="seq1">
              <Elements>
                <Response Role="FLORIAN">
                  <Text Text="Vole, tohle pivo je nějaký divný. Chutnal si ho?" />
                </Response>
                <Response Role="POSLUCHAC_TROSKY_8">
                  <Text Text="Jojo, za chvíli ho pudu asi vyzvracet." />
                </Response>
                <Response Role="POSLUCHAC_TROSKY_8">
                  <Text Text="To víš, chlastáme v tom nejlevnějším pajzlu na testovací placce." />
                </Response>
                <Response Role="POSLUCHAC_TROSKY_8">
                  <Text Text="Lepší matroš tu nenajdeš." />
                </Response>
                <Response Role="FLORIAN">
                  <Text Text="Bůh ví, co za kekel do toho dávají." />
                </Response>
                <Response Role="FLORIAN">
                  <Text Text="Já si snad najdu jinýho skriptéra, kterýmu budu dělat poskoka. Tenhle stojí za hovno." />
                </Response>
              </Elements>
            </Sequence>
          </Sequences>
        </Decision>
      </Dialogue>
    </Dialog>
  </Skald>
