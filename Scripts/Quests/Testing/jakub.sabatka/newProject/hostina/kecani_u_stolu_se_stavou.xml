  <Skald>
    <Dialog Name="kecani_u_stolu_se_stavou">
      <Text Text="Kecání u stolu se šťávou" />
      <Dialogue Type="ingame" TechnicalStatus="Enabled" Initiator="NonPlayer" LocalizationStatus="DontTranslate">
        <Decision Name="dec1" Alias="test_jakub_kecaniUStolu_1">
          <Sequences>
            <Sequence EndType="EndDialogue" Name="seq1">
              <Elements>
                <Response Role="RYCHTAR_HANKO_KOZIHLAVA">
                  <Text Text="Čau šťávo, kam tečeš?" />
                </Response>
                <Response Role="KUCHARKA_FANKA">
                  <Text Text="Mimo tvojí mísu, debile." />
                </Response>
                <Response Role="RYCHTAR_HANKO_KOZIHLAVA">
                  <Text Text="Ty jsi mi nějaká odvařená." />
                </Response>
                <Response Role="KUCHARKA_FANKA">
                  <Text Text="Protože tlačíš moc na plyn." />
                </Response>
                <Response Role="RYCHTAR_HANKO_KOZIHLAVA">
                  <Text Text="To víš, plyn nám po dlouhé době zlevnili, tak konečně můžu trochu přiložit." />
                </Response>
              </Elements>
            </Sequence>
          </Sequences>
        </Decision>
      </Dialogue>
    </Dialog>
  </Skald>
