                  <OutputBreakpoints />
                </Expression>
                <For hasInputBreakpoint="0" comment="" breakpointFlags="0">
                  <OutputBreakpoints />
                  <DistanceCondition hasInputBreakpoint="0" comment="The participants shouldnt be too far from each other" breakpointFlags="0">
                    <OutputBreakpoints />
