<?xml version="1.0" encoding="us-ascii"?>
<ReplayConfig>
	<Behaviors>
		<!-- names are case sensitive! -->
		<!-- name-name of behavior, template-name of SA/SO template, recordedCount-only first n NPCs will be recorded. Infinite can be set by -1. -->
