<!-- 
Description: Automated walktrough on horse. -->


<Commands>

	<!--Setup-->
	<WaitUntilReady/>
	<Wait seconds="25"/>
	<WaitUntilReady/>
	<Console command="wh_ai_MessagePeakEnabled 0"/>
	<Console command="wh_ui_faderdebugalpha 0.5"/>
	<Console command="#haste:RunCommandWithAlias('rpgBuffImortalityHeal')"/>
	<Console command="#haste:RunCommandWithAlias('noExhaustionDigestion')"/>
	<Console command="#haste:RunCommandWithAlias('infinitStamina')"/>
	<WaitUntilReady/>
	<Wait seconds="5"/>
	<Console command="#haste:RunCommandWithAlias('horseWalkthrough')"/>


</Commands>